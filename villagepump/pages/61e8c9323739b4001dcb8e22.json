      "updated": 1642654900
    },
    {
      "id": "61ea7fa0aff09e0000982e59",
      "text": "　持ってる[nishio.icon][erniogi.icon][blu3mo.icon][dai.icon][基素.icon][kidooom.icon][cFQ2f7LRuLYP.icon]",
      "userId": "60508ed36a7abc00235e90c0",
