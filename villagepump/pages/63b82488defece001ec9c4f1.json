      "created": 1673012388,
      "updated": 1673169422
    },
    {
      "id": "63b824a479e11300002700b1",
      "text": "   linear-gradient(var(--A) 8px, transparent 8px) 0 -4px,",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1673012388,
      "updated": 1673012436
    },
    {
      "id": "63b824a479e11300002700b2",
      "text": "   linear-gradient(90deg, var(--A) 8px, transparent 8px) -4px 0;",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1673012388,
      "updated": 1673012438
    },
    {
      "id": "63b824a479e11300002700b3",
