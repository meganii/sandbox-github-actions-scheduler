      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b8249d79e11300002700a5",
      "text": "   --A: var(--card-bg, #D8D8D8);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b8249d79e11300002700a6",
      "text": "   --B: var(--body-bg, #E0E0E0);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "663324e81280f00000d51482",
