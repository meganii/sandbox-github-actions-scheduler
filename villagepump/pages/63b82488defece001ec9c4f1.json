      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b824a279e11300002700ae",
      "text": "   background:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b824a479e11300002700af",
      "text": "     radial-gradient(circle, transparent 20%, var(--B) 20%, var(--B) 80%, transparent 80%, transparent),",
      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b824a479e11300002700b0",
      "text": "     radial-gradient(circle, transparent 20%, var(--B) 20%, var(--B) 80%, transparent 80%, transparent) 50px 50px,",
      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b824a479e11300002700b1",
      "text": "     linear-gradient(var(--A) 8px, transparent 8px) 0 -4px,",
      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b824a479e11300002700b2",
      "text": "     linear-gradient(90deg, var(--A) 8px, transparent 8px) -4px 0;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b824a479e11300002700b3",
      "text": "   background-color: var(--B);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b824a479e11300002700b4",
      "text": "   background-size:100px 100px, 100px 100px, 50px 50px, 50px 50px;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1771238982,
      "updated": 1771238982
    },
    {
      "id": "63b8249d79e11300002700ad",
