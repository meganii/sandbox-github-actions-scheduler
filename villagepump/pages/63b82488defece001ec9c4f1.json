      "created": 1673012382,
      "updated": 1673012382
    },
    {
      "id": "63b8249d79e11300002700a5",
      "text": "   --A: var(--card-bg, #D8D8D8);",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1673012382,
      "updated": 1673012382
    },
    {
      "id": "63b8249d79e11300002700a6",
      "text": "   --B: var(--body-bg, #E0E0E0);",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1673012382,
      "updated": 1673012382
    },
    {
      "id": "63b824a279e11300002700ae",
      "text": "   background:",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1673012386,
      "updated": 1673012388
    },
    {
      "id": "63b824a479e11300002700af",
      "text": "   radial-gradient(circle, transparent 20%, var(--B) 20%, var(--B) 80%, transparent 80%, transparent),",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1673012388,
      "updated": 1673169412
    },
    {
      "id": "63b824a479e11300002700b0",
      "text": "   radial-gradient(circle, transparent 20%, var(--B) 20%, var(--B) 80%, transparent 80%, transparent) 50px 50px,",
