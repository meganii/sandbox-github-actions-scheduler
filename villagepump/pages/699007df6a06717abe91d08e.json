      "updated": 1771046882
    },
    {
      "id": "69900999000000000083c194",
      "text": "[グレーゾーン]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1771047322,
      "updated": 1771047336
    },
    {
      "id": "699007e1000000000083c18d",
      "text": "[誹謗中傷]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1771046882,
      "updated": 1771046893
    },
    {
      "id": "699007e5000000000083c18f",
      "text": "[迷惑行為]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1771046886,
      "updated": 1771046890
    },
    {
      "id": "699007e3000000000083c18e",
      "text": "[荒らし行為]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1771046883,
      "updated": 1771046928
    },
    {
      "id": "69900806000000000083c190",
      "text": "[詐欺]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1771046919,
      "updated": 1771046958
    },
    {
      "id": "6990082b000000000083c191",
      "text": "[著作権侵害]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1771046955,
      "updated": 1771047003
    },
    {
      "id": "69900857000000000083c192",
