    {
      "id": "65237a930e94ff00357248ac",
      "text": "マネジメントサイクル",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824107,
      "updated": 1696824107
    },
    {
      "id": "65237b2a5b98c20000f95a1b",
      "text": "　PDCAサイクルが有名だが、マネジメントサイクルはこの他にもいくつか種類がある",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824107,
      "updated": 1696824172
    },
    {
      "id": "65237ba35b98c20000f95a1f",
      "text": " PDS（plan-do-see）",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824228,
      "updated": 1696824281
    },
    {
      "id": "65237c1a5b98c20000f95a20",
      "text": " 　3つのステップをまわす",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824347,
      "updated": 1696824352
    },
    {
      "id": "65237b2c5b98c20000f95a1c",
      "text": "　[PDCA]（plan-do-check-act）",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824108,
      "updated": 1696824503
    },
    {
      "id": "65237c935b98c20000f95a21",
      "text": "　PDSA（plan-do-study-act）",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824467,
      "updated": 1696824570
    },
    {
      "id": "65237d015b98c20000f95a22",
      "text": "　 PDCAの改良版",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824577,
      "updated": 1696824599
    },
    {
      "id": "65237d175b98c20000f95a23",
      "text": "　 check（確認する）がstudy（学習する）に変わっている",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824600,
      "updated": 1696824654
    },
    {
      "id": "65237b725b98c20000f95a1d",
      "text": "　[OODA]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824179,
      "updated": 1696824213
    },
    {
      "id": "65237ed75b98c20000f95a24",
      "text": "　　想定外の事態に柔軟に対応するためのフレームワーク",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696825048,
      "updated": 1696825093
    },
    {
      "id": "65237b8b5b98c20000f95a1e",
      "text": "　MAIC（measure, analyze, improve, control）",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696824203,
      "updated": 1696825269
    },
    {
      "id": "65237f8c5b98c20000f95a25",
      "text": "　　シックスシグマ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696825228,
      "updated": 1696825284
    },
    {
      "id": "65237ffc5b98c20000f95a29",
      "text": "",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1696825340,
      "updated": 1696825340
    },
    {
      "id": "65237ff05b98c20000f95a27",
      "text": "まだある",
      "userId": "5b5e7121417cf700145b98c2",
