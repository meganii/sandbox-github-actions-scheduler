      "created": 1666161281,
      "updated": 1666161448
    },
    {
      "id": "634f9a7f1280f00000a69a36",
      "text": "      e.preventDefault();",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1666161281,
      "updated": 1666161448
    },
    {
      "id": "634f9a7f1280f00000a69a37",
      "text": "      e.stopPropagation();",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1666161281,
      "updated": 1666161448
    },
    {
      "id": "634f9a7f1280f00000a69a38",
      "text": "      press(\"v\", { shift: true, ctrl: true });",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1666161281,
      "updated": 1666161448
    },
    {
      "id": "634f9a7f1280f00000a69a3a",
      "text": "    });",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1666161281,
      "updated": 1666161448
    },
