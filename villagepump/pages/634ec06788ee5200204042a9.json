      "created": 1666227782,
      "updated": 1666227857
    },
    {
      "id": "63509e451280f00000ce9db6",
      "text": "        if (!hasURL(text)) return;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1666227781,
      "updated": 1666227781
    },
    {
      "id": "63509e161280f00000ce9d92",
      "text": "        e.preventDefault();",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1666227734,
      "updated": 1666227734
    },
    {
      "id": "63509e161280f00000ce9d93",
      "text": "        e.stopPropagation();",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1666227734,
      "updated": 1666227734
    },
    {
      "id": "63509e9e1280f00000ce9dc4",
      "text": "        const newText = await convert(text);",
