      "created": 1668309139,
      "updated": 1668309139
    },
    {
      "id": "637056685b98c200007d7a8b",
      "text": "揺れる",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637057265b98c20000eea498",
      "text": " iconを振動させる記法",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306727,
      "updated": 1668306773
    },
    {
      "id": "637056695b98c200007d7a8c",
      "text": "code:stylexx.css",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668421470
    },
    {
      "id": "637056695b98c200007d7a8d",
      "text": " @keyframes icon-shake {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637056695b98c200007d7a8e",
      "text": "   0%     { transform:translate(0, 0) rotate(0); }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637056695b98c200007d7a8f",
      "text": "   16.66% { transform:translate(2px, -2px) rotate(-5deg); }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637056695b98c200007d7a90",
      "text": "   33.32% { transform:translate(2px, 1px) rotate(5deg); }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637056695b98c200007d7a91",
      "text": "   49.98% { transform:translate(0, 0) rotate(0); }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637056695b98c200007d7a92",
      "text": "   66.64% { transform:translate(-2px, -2px) rotate(5deg); }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637056695b98c200007d7a93",
      "text": "   83.30% { transform:translate(-2px, 3px) rotate(-5deg); }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637056695b98c200007d7a94",
      "text": "   100%   { transform:translate(0, 0) rotate(0); }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637056695b98c200007d7a95",
      "text": " }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668306537,
      "updated": 1668306537
    },
    {
      "id": "637056695b98c200007d7a96",
      "text": " .navbar .navbar-menu>li.stream-btn:hover,",
      "userId": "5b5e7121417cf700145b98c2",
