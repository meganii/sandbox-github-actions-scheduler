    {
      "id": "6218b021774b170000598ab3",
      "text": " ウクライナが気になってしまう",
      "userId": "59fb21121207900012774b17",
      "created": 1645785122,
      "updated": 1645785132
    },
    {
      "id": "6218b02b774b170000598ab4",
