      "created": 1719202673,
      "updated": 1719202931
    },
    {
      "id": "6678f36673e522000054c648",
      "text": " 　     // まとめて挿入",
      "userId": "62919f6444e78d001d73e522",
      "created": 1719202663,
      "updated": 1719202931
    },
    {
      "id": "6678f36673e522000054c649",
      "text": " 　     parent.append(...children);",
      "userId": "62919f6444e78d001d73e522",
      "created": 1719202663,
      "updated": 1719202931
    },
    {
      "id": "6678f36673e522000054c64a",
      "text": " 　 }",
      "userId": "62919f6444e78d001d73e522",
      "created": 1719202663,
      "updated": 1719202931
    },
    {
      "id": "6678f46873e522000054c653",
      "text": " 　だめだった",
      "userId": "62919f6444e78d001d73e522",
      "created": 1719202921,
      "updated": 1719202938
    },
    {
      "id": "6678f47b73e522000054c654",
      "text": " ブロック文じゃなくて[即時実行関数式]にすればいけるか？",
      "userId": "62919f6444e78d001d73e522",
      "created": 1719202939,
      "updated": 1719202989
    },
