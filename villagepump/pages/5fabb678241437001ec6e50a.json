      "updated": 1605089058
    },
    {
      "id": "5fabb7035b98c20000536da6",
      "text": "",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1605089027,
      "updated": 1605089027
    },
    {
      "id": "5fabb69f5b98c20000b85e5c",
      "text": "↓アイコン用画像",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1605088927,
      "updated": 1605088927
    },
    {
      "id": "5fb09a6d5b98c2000024171d",
      "text": "[https://gyazo.com/af16333c1a931f605a96298f5a9cfd8b]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1605409389,
      "updated": 1605409390
    },
    {
      "id": "5fabb69f5b98c20000b85e5d",
      "text": "",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1605088927,
      "updated": 1605409626
    },
    {
      "id": "5fabb69f5b98c20000b85e5e",
      "text": "",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1605088927,
      "updated": 1605088927
    },
    {
      "id": "5fabb69f5b98c20000b85e5f",
      "text": "code:button.js",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1605088927,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123260",
      "text": " (() => {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123261",
      "text": "   let P5js = document.getElementById('__P5js__')",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123262",
      "text": "   if (!P5js){",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123263",
      "text": "     P5js = document.createElement(\"script\");",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123264",
      "text": "     P5js.id = '__P5js__';",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123265",
      "text": "     P5js.src = '//cdnjs.cloudflare.com/ajax/libs/p5.js/1.1.9/p5.min.js';",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123266",
      "text": "     document.head.appendChild(P5js);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123267",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123268",
      "text": "   let script =  document.getElementById('__P5sc__')",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123269",
      "text": "   if (script){",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c2000012326a",
      "text": "     // リロードしてリセット",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c2000012326b",
      "text": "     location.reload(false);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c2000012326c",
      "text": "   } else {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c2000012326d",
      "text": "     script = document.createElement(\"script\");",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c2000012326e",
      "text": "     script.id = '__P5sc__';",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c2000012326f",
      "text": "     script.type = 'module';",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123270",
      "text": "     script.type = 'text/javascript';",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123271",
      "text": "     script.src = `https://scrapbox.io/api/code/${scrapbox.Project.name}/${encodeURIComponent(scrapbox.Page.title)}/sketch.js`",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123272",
      "text": "     document.head.appendChild(script);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123273",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123274",
      "text": " })()",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fccbd8f5b98c20000123275",
      "text": "",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253391,
      "updated": 1607253391
    },
    {
      "id": "5fc756385b98c200002defe5",
      "text": "動的importを使用したversion",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1606899257,
      "updated": 1606899258
    },
    {
      "id": "5fc754105b98c200002a4eeb",
      "text": "↓remove()がうまく動いていないみたい（リロードでリセットできる）",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1606898704,
      "updated": 1606899209
    },
    {
      "id": "5fc753e65b98c200002a4ec6",
      "text": "code:button2.js",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1606898662,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123276",
      "text": " import {installCDN} from '/api/code/villagepump/install-CDN/script.js';",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123277",
      "text": " ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123278",
      "text": " export async function execute() {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123279",
      "text": "   await installCDN({id: '__P5js__', src: '//cdnjs.cloudflare.com/ajax/libs/p5.js/1.1.9/p5.min.js'});",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012327a",
      "text": "   ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012327b",
      "text": "   const p5Script = new P5Script('__P5sc__');",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012327c",
      "text": "   if(p5Script.hasScript()) {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012327d",
      "text": "     p5Script.remove();",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012327e",
      "text": "     alert(\"removed\");",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012327f",
      "text": "     // remove()はDOMツリーから削除するだけなのでリロードする",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123280",
      "text": "     location.reload(false); //",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123281",
      "text": "     return;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123282",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123283",
      "text": "   p5Script.draw();",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123284",
      "text": " }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123285",
      "text": " ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123286",
      "text": " class P5Script {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123287",
      "text": "   constructor(id) {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123288",
      "text": "  \tthis.id = id;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123289",
      "text": "     //console.log(id);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012328a",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012328b",
      "text": "   draw() {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012328c",
      "text": "     const p5script = document.createElement(\"script\");",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012328d",
      "text": "     p5script.id = this.id;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012328e",
      "text": "     p5script.src = `https://scrapbox.io/api/code//${scrapbox.Project.name}/${encodeURIComponent(scrapbox.Page.title)}/sketch.js` ;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012328f",
      "text": "     document.body.appendChild(p5script);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123290",
      "text": "   } ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123291",
      "text": "   hasScript() { return document.getElementById(this.id) !== null;}",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123292",
      "text": "         ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123293",
      "text": "   remove() {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123294",
      "text": "     const p5script = document.getElementById(this.id);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123295",
      "text": "     console.log(p5script);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123296",
      "text": "     if (!p5script) return;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123297",
      "text": "     // 既にscriptがある場合は削除する",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123298",
      "text": "     p5script.parentNode.removeChild(p5script);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c20000123299",
      "text": "     console.log('The script already exists. Removing it...');",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012329a",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012329b",
      "text": " }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012329c",
      "text": " ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdd65b98c2000012329d",
      "text": "",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253462,
      "updated": 1607253462
    },
    {
      "id": "5fccbdea5b98c2000012329e",
      "text": "code:script.js ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253482,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c26",
      "text": " scrapbox.PageMenu.addItem({",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c27",
      "text": "   title: \"p5-sketch\",",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c28",
      "text": "   image: \"https://gyazo.com/af16333c1a931f605a96298f5a9cfd8b/raw\",",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c29",
      "text": "   onClick: async () => {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c2a",
      "text": "　    const path = '//cdnjs.cloudflare.com/ajax/libs/p5.js/1.1.9/p5.min.js';",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c2b",
      "text": "　    await import(path).then(() => {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c2c",
      "text": "       const p5Script = new P5Script('__P5sc__');",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c2d",
      "text": "       if(p5Script.hasScript()) {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c2e",
      "text": "         p5Script.remove();",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c2f",
      "text": "         alert(\"removed\");",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c30",
      "text": "         // remove()はDOMツリーから削除するだけなのでリロードする",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c31",
      "text": "         location.reload(false); //",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c32",
      "text": "         return;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c33",
      "text": "       }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c34",
      "text": "       p5Script.draw();　    ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c35",
      "text": "     });",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c36",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c37",
      "text": " });",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c38",
      "text": " class P5Script {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c39",
      "text": "   constructor(id) {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c3a",
      "text": "  \tthis.id = id;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c3b",
      "text": "     //console.log(id);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c3c",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c3d",
      "text": "   draw() {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c3e",
      "text": "     const p5script = document.createElement(\"script\");",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c3f",
      "text": "     p5script.id = this.id;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c40",
      "text": "     p5script.src = `https://scrapbox.io/api/code//${scrapbox.Project.name}/${encodeURIComponent(scrapbox.Page.title)}/sketch.js` ;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c41",
      "text": "     document.body.appendChild(p5script);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c42",
      "text": "   } ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c43",
      "text": "   hasScript() { return document.getElementById(this.id) !== null;}",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c44",
      "text": "         ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c45",
      "text": "   remove() {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c46",
      "text": "     const p5script = document.getElementById(this.id);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c47",
      "text": "     console.log(p5script);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c48",
      "text": "     if (!p5script) return;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c49",
      "text": "     // 既にscriptがある場合は削除する",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c4a",
      "text": "     p5script.parentNode.removeChild(p5script);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c4b",
      "text": "     console.log('The script already exists. Removing it...');",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c4c",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c4d",
      "text": " }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fccbfbd5b98c20000803c4e",
      "text": "",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253950,
      "updated": 1607253950
    },
    {
      "id": "5fb09be15b98c200001f94ba",
      "text": "p5.jsのスケッチは、インスタンスモードで記述しなければならない。",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1605409761,
      "updated": 1605409762
    },
    {
      "id": "5fabb69f5b98c20000b85e7c",
      "text": "",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1605088927,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524e3",
      "text": "code:sketch.js",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524e4",
      "text": " const sketch = p => {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524e5",
      "text": "   p.setup = () => {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524e6",
      "text": "     //キャンバスを作成",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524e7",
      "text": "     const canvas = p.createCanvas(600,425);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524e8",
      "text": "     //背景色",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253698
    },
    {
      "id": "5fccbeba5b98c200002524ea",
      "text": "     p.background(0);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524eb",
      "text": "     p.noStroke();",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524ec",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524ed",
      "text": "   p.draw = () => {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524ee",
      "text": "     //オブジェクトの色",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524ef",
      "text": "     p.fill(255,255,0,127);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f0",
      "text": "     //キャンバスの中心に直径100pxの丸を描画",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f1",
      "text": "     p.ellipse(p.random(p.width),p.random(p.height),100);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f2",
      "text": "     p.fill(240,240,240);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f3",
      "text": "     let s = 'Scrapbox上からp5.jsが走るよ！';",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f4",
      "text": "     p.textSize(40);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f5",
      "text": "     p.text(s, 0, 0, p.width, p.height);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f6",
      "text": "     ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f7",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f8",
      "text": " }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
    {
      "id": "5fccbeba5b98c200002524f9",
      "text": " new p5(sketch, 'editor');",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607253690,
      "updated": 1607253690
    },
