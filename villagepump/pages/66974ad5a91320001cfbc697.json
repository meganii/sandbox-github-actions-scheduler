      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beeccb",
      "text": "           <source src=\"★★★動画ファイルへのパス★★★\">",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721363575
    },
    {
      "id": "6699e73971b3c20000beeccc",
      "text": "       </video>",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362252
    },
    {
      "id": "6699e73971b3c20000beeccd",
      "text": "       <form id=\"form\">",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecce",
      "text": "           <input type=\"text\" id=\"subt_insert\" size=\"40\" oninput=\"aa2()\">",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beeccf",
      "text": "       </form>",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd0",
      "text": "       <textarea id=\"subt\" cols=\"30\" rows=\"10\">WEBVTT",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd1",
      "text": "   ",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd2",
      "text": "   </textarea>",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd3",
      "text": "       <script>",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd4",
      "text": "           var toosi = 0",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd5",
      "text": "           var t0 = 0",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd6",
      "text": "           function aa2() {",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd7",
      "text": "               document.getElementById('vid').pause();",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd8",
      "text": "           }",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecd9",
      "text": "           window.addEventListener('beforeunload', (e) => {",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecda",
      "text": "               const message = '入力内容が保存されない可能性があります。ページを離れますか？'",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecdb",
      "text": "               e.preventDefault()",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecdc",
      "text": "               e.returnValue = message",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecdd",
      "text": "               return message",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecde",
      "text": "           })",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecdf",
      "text": "           document.getElementById('form').addEventListener('submit', (e) => {",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece0",
      "text": "               e.preventDefault()",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece1",
      "text": "               aaa()",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece2",
      "text": "           })",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece3",
      "text": "           function aaa() {",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece4",
      "text": "               if (document.getElementById('subt_insert').value !=''){",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece5",
      "text": "                   t = document.getElementById('vid').currentTime",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece6",
      "text": "                   tt = ('000' + String(Math.round(t / 60 / 60))).slice(-2) + ':' + ('000' + String(Math.round((t / 60) % 60))).slice(-2) + ':' + ('000' + String(Math.round(t % 60))).slice(-2) + '.' + ('000' + String(Math.round((t % 1) * 100))).slice(-3)",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece7",
      "text": "                   tt0 = ('000' + String(Math.round(t0 / 60 / 60))).slice(-2) + ':' + ('000' + String(Math.round((t0 / 60) % 60))).slice(-2) + ':' + ('000' + String(Math.round(t0 % 60))).slice(-2) + '.' + ('000' + String(Math.round((t % 1) * 100))).slice(-3)",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece8",
      "text": "                   document.getElementById('subt').value += tt0 + ' --> ' + tt + '\\n' + document.getElementById('subt_insert').value + '\\n\\n'",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beece9",
      "text": "                   document.getElementById('subt_insert').value = \"\"",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecea",
      "text": "               }",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beeceb",
      "text": "               t0 = document.getElementById('vid').currentTime",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecec",
      "text": "               document.getElementById('vid').play();",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beeced",
      "text": "           }",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
    {
      "id": "6699e73971b3c20000beecee",
      "text": "       </script>",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1721362231,
      "updated": 1721362231
    },
