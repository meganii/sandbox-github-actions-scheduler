    },
    {
      "id": "663f714f518c250000f942cd",
      "text": "第1部 観光客の哲学",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942ce",
      "text": "　第1章 観光",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942cf",
      "text": "　第2章 二次創作",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942d0",
      "text": "　第3章 政治とその外部",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942d1",
      "text": "　第4章 二層構造",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1716079004
    },
    {
      "id": "663f714f518c250000f942d2",
      "text": "　第5章 郵便的マルチチュードへ",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942d3",
      "text": "————————————",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942d4",
      "text": "第2部 家族の哲学（導入）",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942d5",
      "text": "　第6章 家族",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942d6",
      "text": "　第7章 不気味なもの",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942d7",
      "text": "　第8章 ドストエフスキーの最後の主体",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
      "id": "663f714f518c250000f942d8",
      "text": "————————————",
      "userId": "649a24da357c4d001c518c25",
      "created": 1715433807,
      "updated": 1715433807
    },
    {
