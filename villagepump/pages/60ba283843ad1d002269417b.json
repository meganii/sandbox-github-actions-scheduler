    },
    {
      "id": "60bb12b58ee92a00004fdfbd",
      "text": "数千人が殺された",
      "userId": "566fa156776c9b11008ee92a",
      "created": 1622872758,
      "updated": 1622872758
    },
    {
      "id": "60bb12b78ee92a00004fdfbe",
