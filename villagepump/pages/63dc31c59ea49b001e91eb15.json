    {
      "id": "63dc320271fa080000715156",
      "text": "認知の歪みのパターン",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375107,
      "updated": 1675375140
    },
    {
      "id": "63dc322c71fa080000715157",
      "text": " [全か無かの思考]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375148,
      "updated": 1675375156
    },
    {
      "id": "63dc322e71fa080000715158",
      "text": " [行き過ぎた一般化]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375151,
      "updated": 1675375159
    },
    {
      "id": "63dc7ffe6eb4060000060657",
      "text": " \t[過度の一般化]",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1675395071,
      "updated": 1675395078
    },
    {
      "id": "63dc80066eb4060000060658",
      "text": " \t[過度の抽象化]?",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1675395079,
      "updated": 1675395083
    },
    {
      "id": "63dc322e71fa080000715159",
      "text": " [心のフィルター]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375151,
      "updated": 1675375162
    },
    {
      "id": "63dc322e71fa08000071515a",
      "text": " [マイナス思考]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375151,
      "updated": 1675375165
    },
    {
      "id": "63dc80256eb4060000060659",
      "text": " \t[ポジティブと評価される人の内面]が役立ちそう[inajob.icon]",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1675395109,
      "updated": 1675395117
    },
    {
      "id": "63dc322e71fa08000071515b",
      "text": " [論理の飛躍]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375151,
      "updated": 1675375172
    },
    {
      "id": "63dc322e71fa08000071515c",
      "text": " [拡大（過小）解釈]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375151,
      "updated": 1675375175
    },
    {
      "id": "63dc322e71fa08000071515d",
      "text": " [感情の理由づけ]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375151,
      "updated": 1675375177
    },
    {
      "id": "63dc322e71fa08000071515e",
      "text": " [「～すべき」思考]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375151,
      "updated": 1675375180
    },
    {
      "id": "63dc322e71fa08000071515f",
      "text": " [レッテル貼り]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375151,
      "updated": 1675375182
    },
    {
      "id": "63dc322e71fa080000715160",
      "text": " [誤った自己責任化（個人化）]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375151,
      "updated": 1675375186
    },
    {
      "id": "63dcc2b455cbd300004a27ec",
