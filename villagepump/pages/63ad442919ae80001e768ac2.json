    },
    {
      "id": "63ad4429aff09e0000e07748",
      "text": "from [クオリアさんの話題]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1672299562,
      "updated": 1672299562
    },
    {
      "id": "63ad4429aff09e0000e07749",
      "text": "第一クールで使わなかったクオリアさんの話題",
      "userId": "582e63d27c56960011aff09e",
      "created": 1672299562,
      "updated": 1672299562
    },
    {
      "id": "63ad4429aff09e0000e0774a",
