      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1603104174,
      "updated": 1603104235
    },
    {
      "id": "5f8d6d9a1280f0000058221f",
      "text": "    --card-description-color: rgba(204, 204, 204, .9);",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1603104174,
      "updated": 1603104235
    },
    {
      "id": "5f8d6d9a1280f00000582220",
