      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1603104174,
      "updated": 1603104235
    },
    {
      "id": "5f8d6d9a1280f00000582221",
      "text": "    --card-description-code-color: #99f;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1603104174,
      "updated": 1603104235
    },
    {
      "id": "5f8d6d9a1280f00000582222",
      "text": "    --card-box-shadow-color: transparent;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1603104174,
      "updated": 1603104235
    },
    {
      "id": "5f8d6d9a1280f00000582223",
