      "created": 1603104174,
      "updated": 1603104235
    },
    {
      "id": "5f8d6d9a1280f0000058220e",
      "text": "    --relation-label-links-bg: #fff;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1603104174,
      "updated": 1603104235
    },
    {
      "id": "5f8d6d9a1280f0000058220f",
      "text": "    --relation-label-links-text: #5e8af7;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1603104174,
      "updated": 1603104235
    },
    {
