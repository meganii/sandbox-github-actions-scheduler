      "text": " ",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb9307",
      "text": " declare const scrapbox: Scrapbox;",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb9308",
      "text": " ",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb9309",
      "text": " export interface LaunchOptions {",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb930a",
      "text": "   /** ピン留めするプロジェクト名 既定は現在のプロジェクト名 */",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb930b",
      "text": "   project?: string;",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb930c",
      "text": "   /** ピン留め更新処理の間隔 既定は`60000` */",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb930d",
      "text": "   interval?: number;",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb930e",
      "text": " }",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb930f",
      "text": " ",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb9310",
