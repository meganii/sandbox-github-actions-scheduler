      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669158170
    },
    {
      "id": "637d50bb7838e30000bb9311",
      "text": "   | Iterable<string>",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb9312",
      "text": "   | (() => Iterable<string> | Promise<Iterable<string>>);",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb9313",
      "text": " ",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb9314",
      "text": " /**",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1669157050,
      "updated": 1669157050
    },
    {
      "id": "637d50bb7838e30000bb9315",
