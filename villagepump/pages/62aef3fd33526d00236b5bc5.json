      "userId": "60508ed36a7abc00235e90c0",
      "created": 1655634961,
      "updated": 1655634962
    },
    {
      "id": "62aefc125e90c0000013940c",
      "text": "  5時まで来たぞ",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1655634962,
      "updated": 1655634976
    },
    {
      "id": "62aefc0a5e90c00000139409",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1655634953,
      "updated": 1655634953
    },
    {
      "id": "62aefc435e90c0000013940e",
      "text": "うおおおおお！！！！！！！",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1655635011,
      "updated": 1655635014
    },
    {
      "id": "62aefc0a5e90c0000013940a",
      "text": "[https://gyazo.com/91391a03d9322afe43f3f5f49e5f0b21]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1655634953,
      "updated": 1655635009
    },
    {
      "id": "62aefc425e90c0000013940d",
      "text": " あと一コマや！！！！",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1655635009,
      "updated": 1655635028
    },
    {
      "id": "62aefc545e90c00000139412",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1655635028,
      "updated": 1655635030
    },
    {
