    },
    {
      "id": "6215b9cf1280f00000b9855d",
      "text": "  \t\t\tないです[takker.icon]",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1645590991,
      "updated": 1645590993
    },
    {
      "id": "6215b9d67838e30000a6b58e",
      "text": "  \t\t\t\t👍[Mijinko_SD.icon]",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1645590997,
      "updated": 1645591005
    },
    {
      "id": "6215b9551280f00000b9855b",
      "text": "  \t\tChromeで壊れた……うそん……",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1645590869,
      "updated": 1645590881
    },
    {
      "id": "6215b9a27838e30000a6b58d",
      "text": "  \t\t\tFirefoxでも崩れました[Mijinko_SD.icon]",
      "userId": "60fb0a79f3364000237838e3",
      "created": 1645590944,
      "updated": 1645590952
    },
    {
      "id": "6215dce1a336820000b01011",
      "text": "  \t\t\t\tBraveでも崩れた...[erniogi.icon]",
      "userId": "5e5b870dd7f3000017a33682",
      "created": 1645599969,
      "updated": 1645599978
    },
    {
      "id": "6215b9621280f00000b9855c",
      "text": "  \t\t儚い夢だった……[takker.icon]",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1645590882,
      "updated": 1645590888
    },
    {
      "id": "6215b93a7838e30000a6b58b",
