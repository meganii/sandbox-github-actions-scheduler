    {
      "id": "63fc9e41eaa19d001c33580e",
      "text": "NUDGE 実践 行動経済学 完全版",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677499971,
      "updated": 1677499971
    },
    {
      "id": "63fc9e43aff09e0000c7ead9",
      "text": "[ナッジ]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677499971,
      "updated": 1677499976
    },
    {
      "id": "63fc9e45aff09e0000c7eada",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677499973,
      "updated": 1677499973
    },
    {
      "id": "63fc9e49aff09e0000c7eadb",
      "text": "雑に1ポモドーロ読む[nishio.icon]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677499978,
      "updated": 1677506030
    },
    {
      "id": "63fc9eb5aff09e0000c7eae2",
      "text": "　ミッションクリープ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500085,
      "updated": 1677500089
    },
    {
      "id": "63fc9e63aff09e0000c7eadd",
      "text": "　リバタリアン・パターナリズム",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500004,
      "updated": 1677500028
    },
    {
      "id": "63fc9ebfaff09e0000c7eae3",
      "text": "　追加",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500096,
      "updated": 1677500097
    },
    {
      "id": "63fc9e82aff09e0000c7eade",
      "text": "　スマート・ディスクロージャー",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500035,
      "updated": 1677500042
    },
    {
      "id": "63fc9e8aaff09e0000c7eadf",
      "text": "　選択エンジン",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500042,
      "updated": 1677500048
    },
    {
      "id": "63fc9ec9aff09e0000c7eae4",
      "text": "　　オンラインの意思決定ツール",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500105,
      "updated": 1677500112
    },
    {
      "id": "63fc9ed7aff09e0000c7eae5",
      "text": "　　検索エンジンのメタファー[nishio.icon]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500120,
      "updated": 1677500126
    },
    {
      "id": "63fc9e91aff09e0000c7eae0",
      "text": "　スラッジ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500049,
      "updated": 1677500051
    },
    {
      "id": "63fc9e9aaff09e0000c7eae1",
      "text": "　選択アーキテクチャ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500059,
      "updated": 1677500066
    },
    {
      "id": "63fc9eebaff09e0000c7eae6",
      "text": "　 個別化されたデフォルト",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500140,
      "updated": 1677500145
    },
    {
      "id": "63fc9ef1aff09e0000c7eae7",
      "text": "　 楽しくできるようにする",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500146,
      "updated": 1677500151
    },
    {
      "id": "63fc9ef7aff09e0000c7eae8",
      "text": "　 キュレーション",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500152,
      "updated": 1677500155
    },
    {
      "id": "63fc9f19aff09e0000c7eae9",
      "text": "　企業",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500185,
      "updated": 1677500190
    },
    {
      "id": "63fc9f1faff09e0000c7eaea",
      "text": "　　従業員、顧客、ライバル企業",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500191,
      "updated": 1677500207
    },
    {
      "id": "63fc9f2faff09e0000c7eaeb",
      "text": "　　人間",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500208,
      "updated": 1677500210
    },
    {
      "id": "63fc9f4faff09e0000c7eaec",
      "text": "　セルフコントロール問題",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500240,
      "updated": 1677500256
    },
    {
      "id": "63fc9f6baff09e0000c7eaed",
      "text": "　　コミットメント戦略",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500267,
      "updated": 1677500277
    },
    {
      "id": "63fc9f75aff09e0000c7eaee",
      "text": "　ホモエコノミクスとホモサピエンス",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500277,
      "updated": 1677500310
    },
    {
      "id": "63fc9fa7aff09e0000c7eaf1",
      "text": "　　なぜナッジが必要？",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500327,
      "updated": 1677500333
    },
    {
      "id": "63fc9f9baff09e0000c7eaef",
      "text": "　　自由な選択",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500315,
      "updated": 1677500319
    },
    {
      "id": "63fc9fa0aff09e0000c7eaf0",
      "text": "　バイアスと誤謬",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500320,
      "updated": 1677500343
    },
    {
      "id": "63fc9fb7aff09e0000c7eaf2",
      "text": "　　楽観と自信過剰",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500344,
      "updated": 1677500362
    },
    {
      "id": "63fc9fd0aff09e0000c7eaf3",
      "text": "　　利得と損失",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500368,
      "updated": 1677500373
    },
    {
      "id": "63fc9fd5aff09e0000c7eaf4",
      "text": "　　現状維持バイアス",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500374,
      "updated": 1677500381
    },
    {
      "id": "63fc9fdeaff09e0000c7eaf5",
      "text": "　　フレーミング",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500382,
      "updated": 1677500384
    },
    {
      "id": "63fc9fe1aff09e0000c7eaf6",
      "text": "　　ファスト&スロー",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500385,
      "updated": 1677500401
    },
    {
      "id": "63fc9ff2aff09e0000c7eaf7",
      "text": "　計画通り↔その場の欲望",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500402,
      "updated": 1677500417
    },
    {
      "id": "63fca002aff09e0000c7eaf8",
      "text": "　　メンタルアカウンティング",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500419,
      "updated": 1677500425
    },
    {
      "id": "63fca040aff09e0000c7eafe",
      "text": "　　　あぶく銭",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500481,
      "updated": 1677500485
    },
    {
      "id": "63fca010aff09e0000c7eaf9",
      "text": "　みんなはどうか",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500433,
      "updated": 1677500439
    },
    {
      "id": "63fca017aff09e0000c7eafa",
      "text": "　　同調",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500440,
      "updated": 1677500446
    },
    {
      "id": "63fca01eaff09e0000c7eafb",
      "text": "　　アイデンティティ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500446,
      "updated": 1677500455
    },
    {
      "id": "63fca027aff09e0000c7eafc",
      "text": "　　多元的無知",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500455,
      "updated": 1677500461
    },
    {
      "id": "63fca02eaff09e0000c7eafd",
      "text": "　　ナッジと社会規範",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500463,
      "updated": 1677500470
    },
    {
      "id": "63fc9e60aff09e0000c7eadc",
      "text": "　選択アーキテクト",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500000,
      "updated": 1677500495
    },
    {
      "id": "63fca04eaff09e0000c7eb00",
      "text": "　　選択設計",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500495,
      "updated": 1677500498
    },
    {
      "id": "63fca058aff09e0000c7eb01",
      "text": "　　自由市場",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500505,
      "updated": 1677500512
    },
    {
      "id": "63fca064aff09e0000c7eb02",
      "text": "　　デフォルト",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500517,
      "updated": 1677500521
    },
    {
      "id": "63fca06baff09e0000c7eb03",
      "text": "　　エラー",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500523,
      "updated": 1677500525
    },
    {
      "id": "63fca077aff09e0000c7eb06",
      "text": "　　　起きてしまうもの",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500536,
      "updated": 1677500538
    },
    {
      "id": "63fca06eaff09e0000c7eb04",
      "text": "　　フィードバック",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500526,
      "updated": 1677500529
    },
    {
      "id": "63fca071aff09e0000c7eb05",
      "text": "　　　間違いの自覚",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500529,
      "updated": 1677500534
    },
    {
      "id": "63fca080aff09e0000c7eb07",
      "text": "　　マッピング",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500544,
      "updated": 1677500547
    },
    {
      "id": "63fca084aff09e0000c7eb08",
      "text": "　　　選択と結果の対応関係",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500549,
      "updated": 1677500556
    },
    {
      "id": "63fca08caff09e0000c7eb09",
      "text": "　　構造化",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500557,
      "updated": 1677500562
    },
    {
      "id": "63fca092aff09e0000c7eb0a",
      "text": "　　インセンティブ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500562,
      "updated": 1677500568
    },
    {
      "id": "63fca09aaff09e0000c7eb0b",
      "text": "　　幕間休憩の設定",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500571,
      "updated": 1677500583
    },
    {
      "id": "63fca0a7aff09e0000c7eb0c",
      "text": "　　キュレーション",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500584,
      "updated": 1677500589
    },
    {
      "id": "63fca0aeaff09e0000c7eb0d",
      "text": "　　楽しさ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500591,
      "updated": 1677500595
    },
    {
      "id": "63fca0b3aff09e0000c7eb0e",
      "text": "　スマートディスクロージャー",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500595,
      "updated": 1677500603
    },
    {
      "id": "63fca0bbaff09e0000c7eb0f",
      "text": "　　読む気にならない情報",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500603,
      "updated": 1677500619
    },
    {
      "id": "63fca0cbaff09e0000c7eb10",
      "text": "　　個人の利用履歴データ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500620,
      "updated": 1677500626
    },
    {
      "id": "63fca0d2aff09e0000c7eb11",
      "text": "　スラッジ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500627,
      "updated": 1677500629
    },
    {
      "id": "63fca0e3aff09e0000c7eb12",
      "text": "　能動的な人は良い選択ができたのか？",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500644,
      "updated": 1677500670
    },
    {
      "id": "63fca0feaff09e0000c7eb13",
      "text": "　広告は選択を誤らせる？",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500670,
      "updated": 1677500679
    },
    {
      "id": "63fca108aff09e0000c7eb14",
      "text": "　　そそのかし",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500680,
      "updated": 1677500683
    },
    {
      "id": "63fca10baff09e0000c7eb15",
      "text": "　つい使いすぎる",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500683,
      "updated": 1677500695
    },
    {
      "id": "63fca129aff09e0000c7eb16",
      "text": "　公知性の原則",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500714,
      "updated": 1677500750
    },
    {
      "id": "63fca049aff09e0000c7eaff",
      "text": "　経験則",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677500490,
      "updated": 1677501253
    },
    {
      "id": "63fca345aff09e0000c7eb18",
      "text": "　　アンカリング",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501253,
      "updated": 1677501257
    },
    {
      "id": "63fca366aff09e0000c7eb1c",
      "text": "　　　リアクタンス",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501287,
      "updated": 1677501290
    },
    {
      "id": "63fca349aff09e0000c7eb19",
      "text": "　　利用可能性",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501257,
      "updated": 1677501262
    },
    {
      "id": "63fca381aff09e0000c7eb1d",
      "text": "　　　アクセシビリティ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501313,
      "updated": 1677501317
    },
    {
      "id": "63fca385aff09e0000c7eb1e",
      "text": "　　　顕著性",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501317,
      "updated": 1677501322
    },
    {
      "id": "63fca34eaff09e0000c7eb1a",
      "text": "　　代表性",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501263,
      "updated": 1677501275
    },
    {
      "id": "63fca365aff09e0000c7eb1b",
      "text": "　ファストスロー",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501285,
      "updated": 1677501445
    },
    {
      "id": "63fca404aff09e0000c7eb1f",
      "text": "　　自動システム",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501445,
      "updated": 1677501449
    },
    {
      "id": "63fca409aff09e0000c7eb20",
      "text": "　　熟慮システム",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501450,
      "updated": 1677501454
    },
    {
      "id": "63fca340aff09e0000c7eb17",
      "text": "　誘惑",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501249,
      "updated": 1677501497
    },
    {
      "id": "63fca43aaff09e0000c7eb22",
      "text": "　動学的不整合",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501499,
      "updated": 1677501509
    },
    {
      "id": "63fca434aff09e0000c7eb21",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1677501492,
      "updated": 1677501492
    }
  ]
}