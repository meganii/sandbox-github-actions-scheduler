      "id": "618ba7441280f00000cbd150",
      "text": "         `[${lightFormat(subWeeks(date, 1), format)}.icon]`,",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636542440,
      "updated": 1643762470
    },
    {
      "id": "618ba7f71280f00000cbd153",
      "text": "         `[${lightFormat(subWeeks(date, 2), format)}.icon]`,",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636542455,
      "updated": 1643762470
    },
    {
      "id": "618ba7f81280f00000cbd154",
      "text": "         `[${lightFormat(subWeeks(date, 3), format)}.icon]`,",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636542456,
      "updated": 1643762470
    },
