      "created": 1636536012,
      "updated": 1636536015
    },
    {
      "id": "618b8ee71280f00000cbd11c",
      "text": " // 一年の経過率を計算するのに必要",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636536039,
      "updated": 1636536060
    },
    {
      "id": "618b8ecf1280f00000cbd11a",
      "text": " import getDaysInYear from \"https://deno.land/x/date_fns@v2.22.1/getDaysInYear/index.ts\";",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636536015,
      "updated": 1636536392
    },
    {
      "id": "618ba5711280f00000cbd145",
      "text": " import getDayOfYear from \"https://deno.land/x/date_fns@v2.22.1/getDayOfYear/index.ts\";",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636541809,
      "updated": 1636541817
    },
    {
      "id": "618b8f9f1280f00000cbd121",
      "text": " // 日付計算に使う",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636536223,
      "updated": 1636536228
    },
    {
      "id": "618b8ee41280f00000cbd11b",
      "text": " import addDays from \"https://deno.land/x/date_fns@v2.22.1/addDays/index.ts\";",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636536037,
      "updated": 1636536400
    },
    {
      "id": "618b8f2e1280f00000cbd11d",
      "text": " import subDays from \"https://deno.land/x/date_fns@v2.22.1/subDays/index.ts\";",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636536110,
      "updated": 1636536401
    },
    {
      "id": "618b8f4b1280f00000cbd11e",
      "text": " import subWeeks from \"https://deno.land/x/date_fns@v2.22.1/subWeeks/index.ts\";",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1636536139,
      "updated": 1636536402
    },
    {
      "id": "618b8f821280f00000cbd11f",
