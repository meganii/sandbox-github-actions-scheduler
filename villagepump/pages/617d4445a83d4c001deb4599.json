    },
    {
      "id": "617d44a55b98c2000085eff0",
      "text": " const mode_id = '__filter__'",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1635599525,
      "updated": 1635599525
    },
    {
      "id": "617d44a55b98c2000085eff1",
      "text": " const change_style = keyword_lists => {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1635599525,
      "updated": 1635599525
    },
    {
      "id": "617d44a55b98c2000085eff2",
      "text": "   // let list = '';",
      "userId": "5e8944948a182900174bb2e2",
      "created": 1635599525,
      "updated": 1635602437
    },
    {
      "id": "617d44a55b98c2000085eff3",
      "text": "   // keyword_lists.forEach((hide) => {",
      "userId": "5e8944948a182900174bb2e2",
      "created": 1635599525,
      "updated": 1635602439
    },
    {
      "id": "617d44a55b98c2000085eff4",
      "text": "   // 　　　　list = list + '[data-page-title*=\"' + hide + '\"],\\n';",
      "userId": "5e8944948a182900174bb2e2",
      "created": 1635599525,
      "updated": 1635602441
    },
    {
      "id": "617d44a55b98c2000085eff5",
      "text": "   // });",
      "userId": "5e8944948a182900174bb2e2",
      "created": 1635599525,
      "updated": 1635602443
    },
    {
      "id": "617d4fa74bb2e20000e64391",
      "text": "   const list = keyword_lists.map((hide) => `[data-page-title*='${hide}']`).join(',\\n');",
      "userId": "5b4ea932de544f0014198270",
      "created": 1635602344,
      "updated": 1635603109
    },
    {
      "id": "617d44a55b98c2000085eff6",
      "text": "   //console.log(list);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1635599525,
      "updated": 1635599525
    },
    {
      "id": "617d44a55b98c2000085eff7",
      "text": "   const style = document.getElementById(mode_id);",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1635599525,
      "updated": 1635607789
    },
    {
      "id": "617d44a55b98c2000085eff8",
      "text": "   style?.remove?.();",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1635599525,
      "updated": 1635607785
    },
    {
