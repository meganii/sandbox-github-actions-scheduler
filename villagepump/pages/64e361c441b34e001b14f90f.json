      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82bd",
      "text": " const clickRandomJumpButton = () => {",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82be",
      "text": "  const button = document.querySelector('.random-jump-button.tool-btn.link-btn');",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82bf",
      "text": " ",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c0",
      "text": "  // ボタンが存在する場合はクリック",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c1",
      "text": "  if (button) {",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c2",
      "text": "    button.click();",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c3",
      "text": "  }",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c4",
      "text": " };",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c5",
      "text": " ",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c6",
      "text": " // 初回実行",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c7",
      "text": " clickRandomJumpButton();",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c8",
      "text": " ",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82c9",
      "text": " // 30秒ごとにボタンをクリック",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1692623301,
      "updated": 1692623301
    },
    {
      "id": "64e361c55e90c00000da82ca",
