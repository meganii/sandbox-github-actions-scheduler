  "lines": [
    {
      "id": "63eefdd81e5bd3001ce7ebc5",
      "text": "オタク六法",
      "userId": "59fb21121207900012774b17",
      "created": 1676606938,
      "updated": 1676606938
    },
    {
      "id": "63ef4a5e774b170000f3917d",
      "text": "[https://gyazo.com/899a73951b241e936121585a0bccd45b]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626526,
      "updated": 1676626526
    },
    {
      "id": "63eefdda774b1700000adc46",
      "text": "https://www.amazon.co.jp/exec/obidos/ASIN/4046050330/",
      "userId": "59fb21121207900012774b17",
      "created": 1676606938,
      "updated": 1676606948
    },
    {
      "id": "63ef4a3d774b170000f390ac",
      "text": "　[小林 航太]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef88ea5e90c00000eeb649",
      "text": " 　[みんなで筋肉体操]でおなじみ",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1676642534,
      "updated": 1676642590
    },
    {
      "id": "63ef4a3d774b170000f390ad",
      "text": "　2022",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef88e85e90c00000eeb648",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1676642533,
      "updated": 1676642533
    },
    {
      "id": "63ef4a3d774b170000f390ae",
      "text": "[https://www.kadokawa.co.jp/product/322008000249/ 「オタク六法」 小林 航太 生活・実用書 - KADOKAWA]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1676626493,
      "updated": 1676642531
    },
    {
      "id": "63ef88e65e90c00000eeb647",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1676642531,
      "updated": 1676642531
    },
    {
      "id": "63ef88e35e90c00000eeb646",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1676642528,
      "updated": 1676642528
    },
    {
      "id": "63ef4a3d774b170000f390af",
      "text": "目次 via Amazon",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b0",
      "text": " CHAPTER 1 作るときの法律",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b1",
      "text": "  作品と作った人の権利を守る 著作権",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b2",
      "text": "   そもそも、なんのためにあるの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b3",
      "text": "   著作物を利用できる 「例外」",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b4",
      "text": "   著作権には期限がある",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b5",
      "text": "   著作権侵害は刑事罰の対象になる",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b6",
      "text": "   利用時は許諾を得るのが原則",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b7",
      "text": "   COLUMN 1 法律はなんのためにあるの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b8",
      "text": "  作品はさまざまなかたちでお金に換えられる財産 著作権の種類",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390b9",
      "text": "   著作権は[財産権]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390ba",
      "text": "   著作権に含まれるさまざまな権利",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390bb",
      "text": "  そもそもそれは著作物？ [著作物]の定義",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390bc",
      "text": "   著作物と認められなければ侵害も認められない",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390bd",
      "text": "   著作物に含まれないもの",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390be",
      "text": "   ニュース記事の見出しは著作物?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390bf",
      "text": "   標語・スローガンは著作物?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390c0",
      "text": "   アイデアは著作物?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390c1",
      "text": "   レシピは著作物?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390c2",
      "text": "  著作者だけが持つ権利 [著作者人格権]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390c3",
      "text": "   公表するかどうか、 表示名などを決める権利",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390c4",
      "text": "   SNS の仕様でうっかり侵害！？",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390c6",
      "text": "   著作者の名誉を害する利用も著作者人格権侵害",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390c7",
      "text": "   COLUMN 2 コピーライトマークの意味",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390c8",
      "text": "  私の作品、パクられた? 複製権と翻案権",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390c9",
      "text": "   丸ごとコピーする[複製]、 別のものを作る[翻案]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390ca",
      "text": "　  要件は[依拠性]と[同一性]・[類似性]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390cb",
      "text": "   似ているかどうかは創作性のある部分を比較して検討する",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390cc",
      "text": "   複製・翻案の判断のポイント",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390cd",
      "text": "   私的使用のための複製はOK",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390ce",
      "text": "  大好きな作品にもっと命を吹き込みたい 二次創作",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390cf",
      "text": "　  二次的著作物とは?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d0",
      "text": "   つまり、「二次創作 =二次的創作物」になるの？",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d1",
      "text": "   二次的著作物の著作権は誰にある？",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d2",
      "text": "   同人誌の作者の権利は保護される？",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d3",
      "text": "  　ガイドラインは同人活動二次創作を行うときのルール",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d4",
      "text": "   「明らかに同人活動の範囲を超えていると判断できるものは禁止」",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d5",
      "text": "   キャラクターのイメージも大事",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d6",
      "text": "  著作権が制限される例外 [引用]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d7",
      "text": "   引用するときのルール",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d8",
      "text": "   本の一部や切り抜きを投稿するのは引用の範囲?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390d9",
      "text": "   [切り抜き動画]は 「引用」 になる？",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390da",
      "text": "  インスパイアされた作品を作りたい パロディ・オマージュ・コラージュ",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390db",
      "text": "   元の作品をリスペクトしていればOK?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390dc",
      "text": "  自分の作品を勝手に投稿された 無断転載",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390dd",
      "text": "   無断転載が侵害する権利",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390de",
      "text": "   [公衆送信権]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390df",
      "text": "   無断転載にあたらないもの",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e0",
      "text": "   刑事責任も追及されるネタバレサイトや要約コンテンツ",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e1",
      "text": "   業務妨害罪",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e2",
      "text": "  どこまでOK? 作品のために素材を使いたい",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e3",
      "text": "   使っていいかわからないものは使わない、 が原則",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e4",
      "text": "   利用規約をしっかり確認する",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e5",
      "text": "  著作権侵害に遭ったときに請求できるもの [損害賠償請求]と[差止請求]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e6",
      "text": "   3つの救済措置",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e7",
      "text": "   財産的な被害の回復 「損害賠償請求」",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e8",
      "text": "   請求できる[損害額の算出方法]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390e9",
      "text": "   侵害行為を止めさせる、 予防する 「差止請求」",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390ea",
      "text": "   名誉回復等の措置請求",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390eb",
      "text": "  機械が作品作り AIと著作権",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390ec",
      "text": "   他人が描いたイラストを勝手にAI に読み込ませていいの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390ed",
      "text": "   自分の作品をAIに学習させるのは止められる?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390ee",
      "text": "   AIに学習させるのはモラル的にどうなの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390ef",
      "text": "   AIに描かせたイラストの著作権は誰にある?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390f1",
      "text": "  AIが描いたイラストで著作権侵害が生じることはある?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390f2",
      "text": "  ロゴなどを守る [商標権]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390f3",
      "text": "   グッズ制作にはライセンス契約が必要",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390f4",
      "text": "   [商標]はなんのためにあるの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390f5",
      "text": "   商標権侵害が発生するのはどんなとき?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390f6",
      "text": "   商標を示す2つのマーク",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390f7",
      "text": "   商標登録はするべき?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390f8",
      "text": "   [商標登録できないもの]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390f9",
      "text": "   |COLUMN 3 商品名や作品名をそのまま使っていいの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390fa",
      "text": "  デザインの権利を守りたい [意匠権]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390fb",
      "text": "   工業デザインを保護する法律",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390fc",
      "text": "  グッズを作って販売したい [当日版権システム]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390fd",
      "text": "   イベント運営が代わりに許諾を得るしくみ",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390fe",
      "text": "  写真を無断で撮影された、公開された [肖像権]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f390ff",
      "text": "   勝手に他人の写真を撮らない!",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39100",
      "text": "   肖像権侵害が認められるかはケース・バイ・ケース",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39101",
      "text": "   [パブリシティ権]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39102",
      "text": "   肖像権ガイドライン",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39103",
      "text": " CHAPTER 2 見る・見せるための法律",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39104",
      "text": "  タダで楽しむのはNG [海賊版]コンテンツ",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39105",
      "text": "   厳罰化した海賊版利用",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39106",
      "text": "   二次創作物の海賊版ダウンロードは違法?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39107",
      "text": "  プレイを見たい、見せたい [ゲーム実況]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39108",
      "text": "   ゲーム実況は著作権的にはアウト?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39109",
      "text": "   ガイドラインを守ろう",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3910a",
      "text": "  えっちなイラストやマンガ [わいせつ物頒布等]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3910b",
      "text": "   「わいせつ」にあたるものってどんなもの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3910c",
      "text": "   ヌードなアート作品は「わいせつ」?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3910d",
      "text": "   [ロリ]・[ショタ]絵と[児童ポルノ禁止法]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626586
    },
    {
      "id": "63ef4a3d774b170000f3910e",
      "text": "  インターネットで暴言を言われた [誹謗中傷]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3910f",
      "text": "   誹謗中傷とは?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39110",
      "text": "   刑事上の名誉毀損・侮辱",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39111",
      "text": "   民事上の名誉毀損・侮辱",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39112",
      "text": "   匿名アカウントに対する名誉毀損は認められる?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39113",
      "text": "   亡くなった人の名誉とプライバシーは守られる?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39114",
      "text": "  「○○するぞ!」と、脅されている [脅迫罪]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39115",
      "text": "   幅広いものが 「[害悪の告知]」にあたる",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39116",
      "text": "   COLUMN 4 [表現の自由]とは?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39117",
      "text": "  違法なことをした相手を特定したい [発信者情報開示請求]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39118",
      "text": "   発信者情報開示請求の2つのステップ",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39119",
      "text": "   発信者情報開示請求が認められる要件",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3911a",
      "text": "   発信者情報開示請求は簡単じゃない",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3911b",
      "text": "   [不正アクセス禁止法]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3911c",
      "text": "  大好きなあの人と近づきたい! [ストーカー規制法]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3911d",
      "text": "   どんな行為がストーカー行為にあたるの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3911e",
      "text": "  悪い人は警察に罰してもらえる? [逮捕]の基本",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3911f",
      "text": "   警察に捕まる=罰を与えてもらえる?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39120",
      "text": "   逮捕は手続の一歩にすぎない",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39121",
      "text": "   [被害届]と[告訴]の違い",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39122",
      "text": "  COLUMN 5 [示談]とは",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39123",
      "text": "  COLUMN 6 成年年齢の引き下げ",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39124",
      "text": " CHAPTER 3 楽しむための法律",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39125",
      "text": "  憧れのキャラクターになる [コスプレ]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39126",
      "text": "   コスプレはとりわけ “グレー”?・",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39127",
      "text": "   衣装そのものは著作物にあたるのか?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39128",
      "text": "   自作の衣装でコスプレ写真集を作るのは著作権法上OK ?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39129",
      "text": "   自作の衣装をコスプレイベントで着るのは著作権法上OK ?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3912a",
      "text": "   商標登録されているキャラクターの衣装の柄を使いたい",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3912b",
      "text": "   ライセンス商品がある場合",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3912c",
      "text": "   COLUMN7 「マリカー」 不正競争事件",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3912e",
      "text": "  勝手に写真を撮られた [盗撮]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3912f",
      "text": "   盗撮の定義",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39130",
      "text": "   盗撮するとどんな罰を受けるの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39131",
      "text": "  借りたものを自分のものにする、捨てる、 売る 貸し借りに関するトラブル",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39132",
      "text": "   「借りパク」は[横領罪]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39133",
      "text": "   貸し借りトラブル別の法的解釈",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39134",
      "text": "   弁償金はいくら払うのが妥当?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39135",
      "text": "   お金の話ができない人には貸さない",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39136",
      "text": "   貸す前の状態を写真に撮っておこう",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39137",
      "text": "   サンプルとポイント [使用貸借契約書]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39138",
      "text": "  イベントで仕事を休みたい [有給休暇]の取得",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39139",
      "text": "   有給は理由なく、いつでも取得していい.",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3913a",
      "text": "   「どうしてもそのときに休まれると困る!｣",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3913b",
      "text": "  オタクを理由にいじめられる [いじめ防止対策推進法]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3913c",
      "text": "   学校でのいやがらせ",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3913d",
      "text": "   職場でのいやがらせ",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3913e",
      "text": "   COLUMN8 多重人格の人が罪を犯したら?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3913f",
      "text": "  撮影ロケ、聖地巡礼をしたい! [不退去罪]、 [建造物侵入罪]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39140",
      "text": "  ・立入禁止の場所に立入るとどんな罪にあたるの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39141",
      "text": "  ・撮影許可を得よう",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39142",
      "text": "  COLUMN 9 ドローンでの撮影",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39143",
      "text": "  迷惑なお客さんが来た [威力業務妨害]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39144",
      "text": "   悪質なクレーマー",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39145",
      "text": "  イベントや会場でけがをした、させた [暴行罪]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39146",
      "text": "   けがをしたかによって罪が変わる",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39147",
      "text": "  イベントが中止になった [チケットの払戻し]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39148",
      "text": "   不可抗力による払戻しができるかは規定の有無による",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39149",
      "text": "   払戻しをしない旨の規定は無効になることもある",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3914a",
      "text": "   払戻しに関する規定がないときは? ･",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3914b",
      "text": "  作品やグッズを買いたい、売りたい [売買契約]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3914c",
      "text": "   契約は約束した時点で成立している",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3914d",
      "text": "   やっぱり取引をやめることはできる?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3914e",
      "text": "   [取消し]と[無効]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3914f",
      "text": "   万引きは軽い犯罪ではない",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39150",
      "text": "  本当に買いたい人が買えない [転売]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39151",
      "text": "   転売は本来、自由に行える",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39152",
      "text": "   不正な転売とは?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39153",
      "text": "   なぜチケットだけが法律で規制されるの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39154",
      "text": "   転売と[古物販売]の違い",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39155",
      "text": "  インターネットでの個人取引で騙された [詐欺罪]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39156",
      "text": "   財産を騙し取る行為が必要",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39157",
      "text": "   [欺もう行為]にあたるのはどんなこと",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39158",
      "text": "   お金以外の財産を騙し取る行為も詐欺にあたる",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39159",
      "text": "  パートナーの趣味についていけない 結婚・ 離婚",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3915a",
      "text": "   [離婚]の方法",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3915b",
      "text": "   離婚が認められるには",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3915c",
      "text": "  宝物のグッズを勝手に捨てられた [器物損壊等罪]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3915d",
      "text": "   人のものを勝手に捨てるのは器物損壊等罪",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3915e",
      "text": "  どちらに権利があるかはっきりさせたい 裁判ってなに?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f3915f",
      "text": "   裁判には民事と刑事がある",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39160",
      "text": "   [立証責任]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39161",
      "text": "   「裁判すれば、 ○○できますか!?」",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39162",
      "text": "  判決の内容を実現させる [強制執行]の申立て",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39163",
      "text": "   裁判に勝ったのに、慰謝料がもらえない!?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39164",
      "text": "   勝訴しても1円にもならないケースもある",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39165",
      "text": "   COLUMN 10 [差押え]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626544
    },
    {
      "id": "63ef4a3d774b170000f39166",
      "text": " CHAPTER 4 仕事をするときの法律",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39167",
      "text": "  フリーランスとして独立したい [個人事業主]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39168",
      "text": "   個人事業主とは?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39169",
      "text": "   [開業届]は出す必要ある?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3916a",
      "text": "   [確定申告]はどうすればいいの?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3916b",
      "text": "   法人にしたほうがよい場合、 しなくてよい場合は?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3916c",
      "text": "    [法人成り]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3916d",
      "text": "   印税とは?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3916e",
      "text": "   COLUMN 11 ヒーローの仕事は業務委託!?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3916f",
      "text": "  代金が支払われない、納品されない 仕事のトラブル･",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39170",
      "text": "   仕事の多くが「[請負契約]」",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39171",
      "text": "   トラブル防止のためには記録が大事",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39172",
      "text": "   [下請法]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39173",
      "text": "   サンプルとポイント [業務委託契約書]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39174",
      "text": "  提供された作品を流用する [二次利用]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39175",
      "text": "   二次使用料を受ける権利",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39176",
      "text": "   COLUMN 12 スパイと法律",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39177",
      "text": "  バレたくない! 申告はどうすれば? [副業]",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39178",
      "text": "   副業は本来、自由に行える",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f39179",
      "text": "   会社に申告はしておくほうがよい",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3917a",
      "text": "   確定申告で副業が知られる?",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    },
    {
      "id": "63ef4a3d774b170000f3917b",
      "text": "",
      "userId": "59fb21121207900012774b17",
      "created": 1676626493,
      "updated": 1676626493
    }
  ]
}