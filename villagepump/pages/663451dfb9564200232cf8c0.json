      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714705012,
      "updated": 1714705012
    },
    {
      "id": "663452751280f000009d6673",
      "text": "         content: \"\\f15c\";",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714705012,
      "updated": 1714705012
    },
    {
      "id": "663452751280f000009d6674",
      "text": "       }",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714705012,
      "updated": 1714705012
    },
    {
      "id": "6634526e1280f000009d666c",
      "text": "     }",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714705006,
      "updated": 1714705007
    },
    {
      "id": "663452081280f000009d6466",
      "text": "   }",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704904,
      "updated": 1714704916
    },
    {
      "id": "663452581280f000009d6578",
      "text": "   ",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704984,
      "updated": 1714704984
    },
    {
      "id": "663452211280f000009d648c",
      "text": "   &:not(.cursor-line) .deco-\\. {",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704928,
      "updated": 1714704937
    },
    {
      "id": "6634522a1280f000009d6491",
      "text": "     a:is(.page-link:not(.icon), .link) span:not(.empty-char-index) + span + span ~ span {",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704937,
      "updated": 1714704938
    },
    {
      "id": "663452211280f000009d648d",
      "text": "       display: inline-block;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704928,
      "updated": 1714704940
    },
    {
      "id": "663452211280f000009d648e",
      "text": "       width: 0;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704928,
      "updated": 1714704940
    },
    {
      "id": "663452211280f000009d648f",
      "text": "       text-indent: -9999px;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704928,
      "updated": 1714704940
    },
    {
      "id": "663452211280f000009d6490",
      "text": "     }",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704928,
      "updated": 1714704940
    },
    {
      "id": "663452331280f000009d6498",
      "text": "     .page-link:not(.icon) span.empty-char-index {",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704947,
      "updated": 1714705050
    },
    {
      "id": "6634529a1280f000009d6676",
      "text": "       ~ span.char-index {",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714705050,
      "updated": 1714705053
    },
    {
      "id": "663452331280f000009d6499",
      "text": "         display: inherit;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704947,
      "updated": 1714705053
    },
    {
      "id": "663452331280f000009d649a",
      "text": "         width: inherit;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704947,
      "updated": 1714705053
    },
    {
      "id": "663452331280f000009d649b",
      "text": "         text-indent: inherit;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704947,
      "updated": 1714705053
    },
    {
      "id": "663452331280f000009d649c",
      "text": "       }",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704947,
      "updated": 1714705053
    },
    {
      "id": "663452421280f000009d64a3",
      "text": "       + span::before {",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714704961,
      "updated": 1714705059
    },
    {
      "id": "663452421280f000009d64a4",
