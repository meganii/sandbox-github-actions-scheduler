      "created": 1613147273,
      "updated": 1613147273
    },
    {
      "id": "6026ade3e5172d0000438c48",
      "text": "例",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1613147618,
      "updated": 1613147620
    },
    {
      "id": "6026adf1e5172d0000438c4b",
      "text": "\t[浸透圧]",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1613147632,
      "updated": 1613147635
    },
    {
      "id": "6026ade6e5172d0000438c49",
      "text": "\t[エントロピー弾性]",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1613147620,
      "updated": 1613147637
    },
    {
      "id": "6026adece5172d0000438c4a",
      "text": "\t[疎水効果]",
