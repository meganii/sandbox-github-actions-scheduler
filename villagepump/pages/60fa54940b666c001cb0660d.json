      "created": 1627018400,
      "updated": 1627018403
    },
    {
      "id": "60fa54a1d9875a0000490337",
      "text": " function sayBaka(){",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1627018403,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d55",
      "text": "  let msg = new SpeechSynthesisUtterance();",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d56",
      "text": "  msg.text = 'バカ';",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d57",
      "text": "  msg.lang = 'ja-JP';",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d58",
      "text": "  msg.pitch = Math.random();",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d59",
      "text": "  msg.rate = Math.random();",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d5a",
      "text": "  window.speechSynthesis.speak(msg);",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d5b",
      "text": "  delay=Math.floor(Math.random()*100000)",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d5c",
      "text": "  setTimeout(sayBaka,delay)",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d5d",
      "text": " }",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "62623f8d895f8c0000854d5e",
      "text": " sayBaka()",
      "userId": "6231f8cd37b7b8001d895f8c",
      "created": 1650605966,
      "updated": 1650605966
    },
    {
      "id": "60fa549fd9875a0000490336",
      "text": "",
      "userId": "6231f8cd37b7b8001d895f8c",
