    {
      "id": "696051bf6f26f21fc321eebc",
      "text": "境界線上のホライゾン",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920089,
      "updated": 1767920089
    },
    {
      "id": "6960540600000000002bc158",
      "text": "\t作者：[川上稔]",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920646,
      "updated": 1767920652
    },
    {
      "id": "6960526d00000000002bc150",
      "text": "\t略称：[境ホラ]",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920238,
      "updated": 1767920266
    },
    {
      "id": "696051d800000000002bc145",
      "text": "\t巻数：全32巻",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920089,
      "updated": 1767920263
    },
    {
      "id": "696052f300000000002bc153",
      "text": "\t\t続編『[境界線上のホライゾン NEXT BOX]』連載中",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920372,
      "updated": 1767920388
    },
    {
      "id": "6960550100000000002bc159",
      "text": "\t\t[電子書籍]ver.あり",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920897,
      "updated": 1767920909
    },
    {
      "id": "6960550d00000000002bc15a",
      "text": "\t\t\t数年前までなかった",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920909,
      "updated": 1767920922
    },
    {
      "id": "6960520800000000002bc14b",
      "text": "\t[アニメ]化している（全2期26話）",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920137,
      "updated": 1767920173
    },
    {
      "id": "6960522f00000000002bc14c",
      "text": "\t1冊1冊が設定資料等が書き込まれていて分厚い",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920175,
      "updated": 1767920212
    },
    {
      "id": "6960552900000000002bc15d",
      "text": "\t\t写真中のⅡ（下）のページ数：1,187",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920937,
      "updated": 1767920939
    },
    {
      "id": "696052c400000000002bc152",
      "text": "\t\t1冊単体で自立可能",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920325,
      "updated": 1767920348
    },
    {
      "id": "6960525300000000002bc14d",
      "text": "\t\t鈍器と呼ばれている",
      "userId": "65d0d54ba4356b00242d00f4",
      "created": 1767920212,
      "updated": 1767920221
    },
    {
      "id": "6960526000000000002bc14e",
