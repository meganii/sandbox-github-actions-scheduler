      "updated": 1652201494
    },
    {
      "id": "627a981d55cbd300001cf680",
      "text": " .deco--.deco-\\! img.icon {",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1652201494,
      "updated": 1652201494
    },
    {
      "id": "627a981d55cbd300001cf681",
      "text": " \tanimation: 1.0s icon-rotate-shake linear infinite;",
      "userId": "5ae7fecf7766b7001455cbd3",
