    {
      "id": "65d71a96d5fa570026f451c7",
      "text": "セパレーター記法",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595866,
      "updated": 1708595866
    },
    {
      "id": "65d71a9affff360000c7b91c",
      "text": "from [/per-terra/セパレーター記法]",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595867,
      "updated": 1708595873
    },
    {
      "id": "65d71a9effff360000c7b91d",
      "text": "",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595871,
      "updated": 1708595871
    },
    {
      "id": "65d71ca4ffff360000748cd4",
      "text": " 名前はこれでいいのだろうか[per_terra.icon]",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708596388,
      "updated": 1708596400
    },
    {
      "id": "65d71b68ffff360000c7b93b",
      "text": " [中央揃え記法]がすでに存在するのでそれと`_`とのコンビネーションで発動するようにしてみる",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708596073,
      "updated": 1708596387
    },
    {
      "id": "65d74887ffff3600006b8983",
      "text": " 加えて`-`をつけると破線になる",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708607624,
      "updated": 1708607640
    },
    {
      "id": "65d71b69ffff360000c7b93c",
      "text": "",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708596073,
      "updated": 1708596073
    },
    {
      "id": "65d71aa2ffff360000c7b91e",
      "text": "code:style.css",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595875,
      "updated": 1708595886
    },
    {
      "id": "65d71aadffff360000c7b920",
      "text": " div.line:not(.cursor-line)>span.text>span>span.deco:has(>span>strike>span.deco-\\'.deco-_),",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1708595886,
      "updated": 1742601819
    },
    {
      "id": "67ddfd6271b3c200008b7b92",
      "text": " div.line:not(.cursor-line)>span.text>span>span.deco:has(>span>span.deco-\\'.deco-_) {",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1742601571,
      "updated": 1742601827
    },
    {
      "id": "65d71aaeffff360000c7b921",
      "text": " \tdisplay: flex;",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b922",
      "text": " \talign-items: center;",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b923",
      "text": " \ttext-align: center;",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b924",
      "text": " ",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b925",
      "text": " \t&::before, &::after {",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b926",
      "text": " \t\tflex-grow: 1;",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b927",
      "text": " \t\tmin-width: 30px;",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b928",
      "text": " \t\tcontent: \"\";",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b929",
      "text": " \t\tborder-top: 1px solid #afafaf;",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b92a",
      "text": " \t}",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d747c5ffff36000056205d",
      "text": " \t",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708607429,
      "updated": 1708607429
    },
    {
      "id": "65d747c5ffff36000056205e",
      "text": " \t&:has(.deco-\\-) {",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708607429,
      "updated": 1708607448
    },
    {
      "id": "65d747d8ffff36000056205f",
      "text": " \t\t&::before, &::after {",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708607448,
      "updated": 1708607460
    },
    {
      "id": "65d747e4ffff360000562061",
      "text": " \t\t\tborder-style: dashed none none;",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708607460,
      "updated": 1708607460
    },
    {
      "id": "65d747e4ffff360000562062",
      "text": " \t\t}",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708607460,
      "updated": 1708607460
    },
    {
      "id": "65d747d8ffff360000562060",
      "text": " \t}",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708607449,
      "updated": 1708607449
    },
    {
      "id": "65d71aaeffff360000c7b92b",
      "text": " ",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708607426
    },
    {
      "id": "65d71aaeffff360000c7b92c",
      "text": " \t&::before {",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b92d",
      "text": " \t\tmargin-right: 10px;",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b92e",
      "text": " \t}",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b92f",
      "text": " ",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708607425
    },
    {
      "id": "65d71aaeffff360000c7b930",
      "text": " \t&::after {",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b931",
      "text": " \t\tmargin-left: 10px;",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aaeffff360000c7b932",
      "text": " \t}",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "67ddfcfd71b3c20000d3a91b",
      "text": " \t",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1742601470,
      "updated": 1742601470
    },
    {
      "id": "67ddfcfe71b3c20000d3a91c",
      "text": " \t&>span>strike, &>span{",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1742601471,
      "updated": 1742601953
    },
    {
      "id": "65d71ad4ffff360000c7b936",
      "text": " \t\ttext-decoration: none !important;",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1708595924,
      "updated": 1742601476
    },
    {
      "id": "67ddfd0271b3c20000d3a91d",
      "text": " \t}",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1742601474,
      "updated": 1742601474
    },
    {
      "id": "65d71aaeffff360000c7b933",
      "text": " }",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595887,
      "updated": 1708595887
    },
    {
      "id": "65d71aa2ffff360000c7b91f",
      "text": "",
      "userId": "5f1d18b112e799001effff36",
      "created": 1708595875,
      "updated": 1708595875
    },
    {
      "id": "65d71be1ffff360000c7b93d",
