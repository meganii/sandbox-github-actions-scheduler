      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1731387636,
      "updated": 1731387641
    },
    {
      "id": "6732e3d7c9ec6a000009400e",
      "text": "　 グワー[cak.icon]",
      "userId": "65b91ba0863c690027c9ec6a",
      "created": 1731388375,
      "updated": 1731388380
    },
    {
      "id": "67338f4bb8bbfc0000d4dede",
      "text": "　終わってる",
      "userId": "60c9f2f6c225a50020b8bbfc",
      "created": 1731432267,
      "updated": 1731432269
    },
    {
      "id": "67338fdab8bbfc0000d4dee1",
