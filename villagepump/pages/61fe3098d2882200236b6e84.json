      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1644048543,
      "updated": 1644048543
    },
    {
      "id": "61fe309e1280f000003e3355",
      "text": " .line .telomere .telomere-border.unread {",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1644048543,
      "updated": 1644048543
    },
    {
      "id": "61fe309e1280f000003e3356",
      "text": "   background-color: var(--telomere-unread, #52ba68);",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1644048543,
      "updated": 1644048543
    },
    {
