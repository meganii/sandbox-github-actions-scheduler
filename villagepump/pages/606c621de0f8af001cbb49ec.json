    {
      "id": "606c62f71280f00000fd60b5",
      "text": "[https://plantuml-proxy.vercel.app/svg/https://scrapbox.io/api/code/villagepump/PlantUMLで遊ぶ/aws.uml#.svg]",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715958,
      "updated": 1617715968
    },
    {
      "id": "606c623b1280f00000fd6087",
      "text": "from https://plantuml.com/ja/stdlib",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715771,
      "updated": 1617715783
    },
    {
      "id": "606c622e1280f00000fd6068",
      "text": "code:aws.uml",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715757,
      "updated": 1617715888
    },
    {
      "id": "606c62381280f00000fd6069",
      "text": " @startuml",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715767,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd606a",
      "text": " 'Copyright 2019 Amazon.com, Inc. or its affiliates. All Rights Reserved.",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd606b",
      "text": " 'SPDX-License-Identifier: MIT (For details, see https://github.com/awslabs/aws-icons-for-plantuml/blob/master/LICENSE)",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd606c",
      "text": " ",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd606d",
      "text": " !include <awslib/AWSCommon>",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd606e",
      "text": " ",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd606f",
      "text": " ' Uncomment the following line to create simplified view",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6070",
      "text": " ' !include <awslib/AWSSimplified>",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6071",
      "text": " ",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6072",
      "text": " !include <awslib/General/Users>",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6073",
      "text": " !include <awslib/Mobile/APIGateway>",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6074",
      "text": " !include <awslib/SecurityIdentityAndCompliance/Cognito>",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6075",
      "text": " !include <awslib/Compute/Lambda>",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6076",
      "text": " !include <awslib/Database/DynamoDB>",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6077",
      "text": " ",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6078",
      "text": " left to right direction",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6079",
      "text": " ",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd607a",
      "text": " Users(sources, \"Events\", \"millions of users\")",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd607b",
      "text": " APIGateway(votingAPI, \"Voting API\", \"user votes\")",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd607c",
      "text": " Cognito(userAuth, \"User Authentication\", \"jwt to submit votes\")",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd607d",
      "text": " Lambda(generateToken, \"User Credentials\", \"return jwt\")",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd607e",
      "text": " Lambda(recordVote, \"Record Vote\", \"enter or update vote per user\")",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd607f",
      "text": " DynamoDB(voteDb, \"Vote Database\", \"one entry per user\")",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6080",
      "text": " ",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6081",
      "text": " sources --> userAuth",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6082",
      "text": " sources --> votingAPI",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6083",
      "text": " userAuth <--> generateToken",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6084",
      "text": " votingAPI --> recordVote",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6085",
      "text": " recordVote --> voteDb",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
    {
      "id": "606c62381280f00000fd6086",
      "text": " @enduml",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1617715768,
      "updated": 1617715768
    },
