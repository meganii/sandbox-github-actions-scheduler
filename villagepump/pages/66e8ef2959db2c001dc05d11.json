      "created": 1726541705,
      "updated": 1726541792
    },
    {
      "id": "66e8ef9773e5220000c813aa",
      "text": "   .fill-same-color {",
      "userId": "62919f6444e78d001d73e522",
      "created": 1726541720,
      "updated": 1726541792
    },
    {
      "id": "66e8efa773e5220000c813ac",
      "text": "        background-color: currentcolor;",
      "userId": "62919f6444e78d001d73e522",
      "created": 1726541735,
      "updated": 1726541792
    },
    {
      "id": "66e8efa273e5220000c813ab",
      "text": "   }",
      "userId": "62919f6444e78d001d73e522",
      "created": 1726541730,
      "updated": 1726541793
    },
    {
      "id": "66e8efe273e5220000c813af",
