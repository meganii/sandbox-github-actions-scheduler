      "created": 1665725501,
      "updated": 1665754568
    },
    {
      "id": "6348f4461982700000eafc9e",
      "text": "\t\t\t論理プログラミング",
      "userId": "5b4ea932de544f0014198270",
      "created": 1665725511,
      "updated": 1665725519
    },
    {
      "id": "6348f44f1982700000eafc9f",
      "text": "\t\t\t項書き換えモデル",
      "userId": "5b4ea932de544f0014198270",
      "created": 1665725519,
      "updated": 1665725528
    },
    {
      "id": "6348f4571982700000eafca0",
      "text": "\t\t\t代数モデル",
