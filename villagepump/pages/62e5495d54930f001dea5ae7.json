      "text": " 　[https://images-na.ssl-images-amazon.com/images/I/6134u-agvDL.jpg https://www.amazon.co.jp/%E9%AD%94%E6%B3%95%E5%B0%91%E5%A5%B3%E3%81%AF%E3%81%AA%E3%81%9C%E5%A4%89%E8%BA%AB%E3%81%99%E3%82%8B%E3%81%AE%E3%81%8B-%E3%83%9D%E3%83%83%E3%83%97%E3%82%AB%E3%83%AB%E3%83%81%E3%83%A3%E3%83%BC%E3%81%AE%E3%81%AA%E3%81%8B%E3%81%AE%E5%AE%97%E6%95%99-%E7%9F%B3%E4%BA%95-%E7%A0%94%E5%A3%AB/dp/439329162X/]",
