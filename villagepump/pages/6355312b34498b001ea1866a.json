      "created": 1666527751,
      "updated": 1666527751
    },
    {
      "id": "63b6cd996eb406000010dd00",
      "text": "現状演奏できる和音",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1672924570,
      "updated": 1672924577
    },
    {
      "id": "63b6cda16eb406000010dd02",
      "text": "\tメジャー",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1672924579,
      "updated": 1672924584
    },
    {
      "id": "63b6cda66eb406000010dd03",
      "text": "\tマイナー",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1672924584,
      "updated": 1672924585
    },
    {
      "id": "63b6cda86eb406000010dd04",
      "text": "\tsus4",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1672924585,
      "updated": 1672924588
    },
    {
      "id": "63b6ce8d6eb406000010dd07",
