  "lines": [
    {
      "id": "65995176ef2a8900231f5486",
      "text": "カードゲームのアドバンテージ",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546683,
      "updated": 1704546687
    },
    {
      "id": "659951805e90c000002524f6",
      "text": "https://note.com/fladdict/n/n5a84fb8cbabd",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546688,
      "updated": 1704546688
    },
    {
      "id": "659951825e90c000002524f8",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546689,
      "updated": 1704546689
    },
    {
      "id": "659951835e90c000002524f9",
      "text": "[ハンド・アドバンテージ]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546691,
      "updated": 1704547054
    },
    {
      "id": "659951895e90c000002524fa",
      "text": " 相手より[手札]が多い",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546697,
      "updated": 1704547251
    },
    {
      "id": "659952475e90c0000025250c",
      "text": "  取れる選択肢が増える",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546887,
      "updated": 1704546895
    },
    {
      "id": "659951935e90c000002524fb",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546707,
      "updated": 1704546707
    },
    {
      "id": "6599519e5e90c000002524fd",
      "text": "[ドロー・アドバンテージ]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546717,
      "updated": 1704547057
    },
    {
      "id": "659951a45e90c000002524fe",
      "text": " 相手より引ける数が多い",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546724,
      "updated": 1704546737
    },
    {
      "id": "659952525e90c0000025250d",
      "text": "  有効牌を引いてきやすい",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546898,
      "updated": 1704546913
    },
    {
      "id": "659951b25e90c000002524ff",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546738,
      "updated": 1704546738
    },
    {
      "id": "659951ba5e90c00000252501",
      "text": "[メタ・アドバンテージ]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546746,
      "updated": 1704547060
    },
    {
      "id": "659951c35e90c00000252502",
      "text": " 相手の相性が良い（構造上有利）",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546755,
      "updated": 1704546777
    },
    {
      "id": "659951db5e90c00000252503",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546779,
      "updated": 1704546779
    },
    {
      "id": "659953415e90c00000b0d385",
      "text": "[カード・アドバンテージ]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704547137,
      "updated": 1704547156
    },
    {
      "id": "659953475e90c00000b0d387",
      "text": " 持ってるカードの性能がいい",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704547143,
      "updated": 1704547154
    },
    {
      "id": "659953415e90c00000b0d386",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704547137,
      "updated": 1704547137
    },
    {
      "id": "659951db5e90c00000252504",
      "text": "[マナ・アドバンテージ]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546779,
      "updated": 1704547062
    },
    {
      "id": "659951f25e90c00000252505",
      "text": " 相手よりも行動ポイントが多い",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546802,
      "updated": 1704546820
    },
    {
      "id": "659952645e90c0000025250e",
      "text": "  対戦が長くなるほどこちらの行動回数が優位になる",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546916,
      "updated": 1704546948
    },
    {
      "id": "659952055e90c00000252506",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546821,
      "updated": 1704546821
    },
    {
      "id": "659952055e90c00000252507",
      "text": "[インフォ・アドバンテージ]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546821,
      "updated": 1704547064
    },
    {
      "id": "6599521b5e90c00000252508",
      "text": " 相手よりも情報を持っている",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546843,
      "updated": 1704546866
    },
    {
      "id": "659952985e90c0000025250f",
      "text": "  相手の次の手やフィニッシャーを予想できる",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546968,
      "updated": 1704546987
    },
    {
      "id": "659952335e90c00000252509",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546866,
      "updated": 1704546866
    },
    {
      "id": "659952335e90c0000025250a",
      "text": "[テンポ・アドバンテージ]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546866,
      "updated": 1704547066
    },
    {
      "id": "659952ad5e90c00000252510",
      "text": " 相手より効率よく動ける",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546989,
      "updated": 1704547005
    },
    {
      "id": "659952be5e90c00000252511",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704547006,
      "updated": 1704547006
    },
    {
      "id": "659952bf5e90c00000252512",
      "text": "[ボード・アドバンテージ]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704547006,
      "updated": 1704547068
    },
    {
      "id": "659952c35e90c00000252513",
      "text": " 相手よりも[盤面]が有利",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704547011,
      "updated": 1704547243
    },
    {
      "id": "659952cf5e90c00000252514",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704547023,
      "updated": 1704547023
    },
    {
      "id": "659952cf5e90c00000252515",
      "text": "[ライフ・アドバンテージ]",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704547023,
      "updated": 1704547071
    },
    {
      "id": "659952dd5e90c00000252516",
      "text": " 相手よりも死に遠い",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704547037,
      "updated": 1704547047
    },
    {
      "id": "659952385e90c0000025250b",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1704546871,
      "updated": 1704546871
    },
