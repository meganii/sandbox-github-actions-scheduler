    },
    {
      "id": "629afc305e90c00000969ef2",
      "text": "　　[/icons/お大事に.icon][cFQ2f7LRuLYP.icon][takker.icon][maichan.icon]",
      "userId": "60508ed36a7abc00235e90c0",
