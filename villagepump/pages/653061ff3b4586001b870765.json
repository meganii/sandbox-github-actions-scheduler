    {
      "id": "653061ff3b4586001b870765",
      "text": "ポッホハマー記号",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669634,
      "updated": 1697669634
    },
    {
      "id": "6530656ced60e60000af266d",
      "text": "[Pochhammer記号][ポッチャマ.icon]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697670508,
      "updated": 1697670520
    },
    {
      "id": "65306201ed60e60000af2656",
      "text": "[特殊関数論]と[組合せ論]で[$ (x)_n]が表す式が違う",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669634,
      "updated": 1697670043
    },
    {
      "id": "65306233ed60e60000af2658",
      "text": "\tヤメロ[ろほぬもめぽよ.icon]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669683,
      "updated": 1697669687
    },
    {
      "id": "65306237ed60e60000af2659",
      "text": "",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669687,
      "updated": 1697669700
    },
    {
      "id": "6530624ced60e60000af265b",
      "text": "[特殊関数論]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669709,
      "updated": 1697669712
    },
    {
      "id": "65306244ed60e60000af265a",
      "text": " [$ (z)_n=\\prod_{j=0}^{n-1}(z+j)]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669700,
      "updated": 1697669753
    },
    {
      "id": "6530633aed60e60000af265e",
      "text": " [$ =z^{\\overline n}]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669946,
      "updated": 1697669966
    },
    {
      "id": "65306428ed60e60000af2664",
      "text": " [$ =\\frac{\\Gamma(x+n)}{\\Gamma(x)}]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697670185,
      "updated": 1697670185
    },
    {
      "id": "65306388ed60e60000af2660",
      "text": " [$ =\\frac{(z+n-1)!}{(z-1)!}]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697670025,
      "updated": 1697670120
    },
    {
      "id": "65306252ed60e60000af265c",
      "text": "[組合せ論]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669714,
      "updated": 1697669718
    },
    {
      "id": "65306258ed60e60000af265d",
      "text": "\t[$ (n)_r=\\prod_{j=0}^{r-1}(n-j)=\\,_n{\\rm P}_{r}]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669720,
      "updated": 1697669873
    },
    {
      "id": "65306353ed60e60000af265f",
      "text": "\t[$ n^{\\underline r}]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669971,
      "updated": 1697669991
    },
    {
      "id": "6530638ded60e60000af2661",
      "text": "\t[$ \\frac{n!}{(n-r)!}]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697670029,
      "updated": 1697670088
    },
    {
      "id": "65306403ed60e60000af2663",
      "text": "\t[$ =\\frac{\\Gamma(n+1)}{\\Gamma(n-r+1)}]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697670148,
      "updated": 1697670176
    },
    {
      "id": "65306219ed60e60000af2657",
      "text": "",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1697669657,
      "updated": 1697669657
    }
  ]
}