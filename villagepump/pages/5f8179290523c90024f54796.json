    {
      "id": "5f817acc4827540000b4597a",
      "text": "     fill=\"#00FF00\", outline=\"\")",
      "userId": "5ee76ce85f51de0023482754",
      "created": 1602321101,
      "updated": 1602321101
    },
    {
      "id": "5f817acc4827540000b4597b",
      "text": " root.mainloop()",
      "userId": "5ee76ce85f51de0023482754",
      "created": 1602321101,
      "updated": 1602321101
    },
    {
