    },
    {
      "id": "66f9b4876eb4060000004fee30",
      "text": "第40週: 日[[月]]火水木金土",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1727640711,
      "updated": 1727640711
    },
    {
      "id": "66f9b4876eb40600000021e894",
      "text": "2024年 74.86％経過",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1727640711,
      "updated": 1727640711
    },
    {
      "id": "66f94a6871b3c2000029c97f",
      "text": "[bsahd.icon]",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1727613545,
      "updated": 1727613545
    },
    {
      "id": "66f94c9c897bef000094acfb",
      "text": "\tお早いですね[star_field.icon]",
      "userId": "640b5ab62ffe46001b897bef",
      "created": 1727614110,
      "updated": 1727614114
    },
    {
      "id": "66f94c9e897bef000094acfc",
      "text": "",
      "userId": "640b5ab62ffe46001b897bef",
      "created": 1727614112,
      "updated": 1727614112
    },
    {
      "id": "66f9b4876eb40600000094247c",
      "text": "今日のn年前",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1727640711,
      "updated": 1727640711
    },
    {
      "id": "66f9b4876eb4060000005a4da9",
      "text": " [2023/09/30]",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1727640711,
      "updated": 1727640711
    },
    {
      "id": "66f9b4876eb406000000af5ca5",
      "text": " [2022/09/30]",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1727640711,
      "updated": 1727640711
    },
    {
      "id": "66f9b4876eb406000000a00d70",
      "text": " [2021/09/30]",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1727640711,
      "updated": 1727640711
    },
    {
      "id": "66f9b4876eb406000000db9cd4",
