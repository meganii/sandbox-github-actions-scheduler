      "created": 1602909137,
      "updated": 1602909138
    },
    {
      "id": "5fb9e7ff1280f00000d093e1",
      "text": " const timestamp = date => `${date.getFullYear()}/${zero(date.getMonth() + 1)}/${zero(date.getDate())}`",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1606019071,
      "updated": 1606019137
    },
