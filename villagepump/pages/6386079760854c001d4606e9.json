      "created": 1669728192,
      "updated": 1669728192
    },
    {
      "id": "638610205e90c00000e1b202",
      "text": "関連がない",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1669730335,
      "updated": 1669730354
    },
    {
      "id": "638610225e90c00000e1b204",
