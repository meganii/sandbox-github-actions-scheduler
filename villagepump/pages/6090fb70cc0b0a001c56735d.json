    {
      "id": "63fec6f279e1130000e87b61",
      "text": "[/nishio/ToMyProj]",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b63",
      "text": "code:script.js",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b64",
      "text": "\tscrapbox.PopupMenu.addButton({",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b65",
      "text": "   title: 'ToMyProj',",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b66",
      "text": "   onClick: text => {",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b67",
      "text": "     const dst = 'blu3mo'",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677642394
    },
    {
      "id": "63fec6f379e1130000e87b68",
      "text": "     const src = scrapbox.Project.name",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b69",
      "text": "     const new_text = text.replace(/\\[([^\\]\\/]+).icon\\]/gm, `[/${src}/$1.icon]`)",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b6a",
      "text": " ",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b6b",
      "text": "     const lines = new_text.split(/[\\r\\n]/g)",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b6c",
      "text": " ",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b6d",
      "text": "     const title = scrapbox.Page.title",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b6e",
      "text": "     lines.unshift(`from [/${src}/${title}]`)",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b6f",
      "text": " ",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b70",
      "text": " \tconst title_url = encodeURIComponent(title)",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b71",
      "text": "     const body = lines.join('\\n')",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b72",
      "text": "     navigator.clipboard.writeText(body).then(()=>{",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b73",
      "text": "       if (window.confirm(\"copied. create page?\")) {",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b74",
      "text": "         const body_enc = encodeURIComponent(body)",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b75",
      "text": "         window.open(`https://scrapbox.io/${dst}/Re:${title_url}?body=${body_enc}`)",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b76",
      "text": "       }",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b77",
      "text": "     })",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b78",
      "text": "   },",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
    {
      "id": "63fec6f379e1130000e87b79",
      "text": " })",
      "userId": "5d89b1f60230ae001779e113",
      "created": 1677641459,
      "updated": 1677641459
    },
