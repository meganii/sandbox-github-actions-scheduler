    {
      "id": "637deef65e90c000009db786",
      "text": "#ファイアーエムブレム",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1669197557,
      "updated": 1669294784
    },
    {
      "id": "637f6abf5e90c0000001c14d",
