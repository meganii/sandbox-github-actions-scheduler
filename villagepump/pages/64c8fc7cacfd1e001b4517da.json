      "created": 1691256016,
      "updated": 1691256016
    },
    {
      "id": "64ce84cfa56fda000020df71",
      "text": "   todayYYYYMMDD, todayHHMMSS, todayYYYYMMDD_HHMMSS",
      "userId": "5eb247ae6474d4001ea56fda",
      "created": 1691256016,
      "updated": 1691256016
    },
    {
      "id": "64ce84cfa56fda000020df72",
      "text": " } from '/api/code/customize/mobileからtimestampを行頭に挿入するUserScript/timestamp.js';",
      "userId": "5eb247ae6474d4001ea56fda",
      "created": 1691256016,
      "updated": 1691256016
    },
    {
      "id": "64ce84cfa56fda000020df73",
      "text": " import {isMobile} from '/api/code/customize/モバイル版Scrapboxかどうかを判定するUserScript/script.js';",
      "userId": "5eb247ae6474d4001ea56fda",
      "created": 1691256016,
      "updated": 1691256016
    },
    {
      "id": "64ce84cfa56fda000020df74",
      "text": " ",
      "userId": "5eb247ae6474d4001ea56fda",
      "created": 1691256016,
      "updated": 1691256016
    },
    {
      "id": "64ce84cfa56fda000020df75",
      "text": " ",
      "userId": "5eb247ae6474d4001ea56fda",
      "created": 1691256016,
      "updated": 1691256016
    },
