      "created": 1730639549,
      "updated": 1730639549
    },
    {
      "id": "672776bee5172d0000483b30",
      "text": " 　 　  export * from \"jsr:@cosense/std\";",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1730639549,
      "updated": 1730639549
    },
    {
      "id": "672776bee5172d0000483b31",
