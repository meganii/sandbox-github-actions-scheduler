    },
    {
      "id": "5fd0d5e55b98c20000a1dee6",
      "text": "     editor.insertAdjacentHTML('beforeend',`",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1dee7",
      "text": "       <div id=\"vis\" style=\"width:600px;height:500px;\" >",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1dee8",
      "text": "       </div>",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1dee9",
      "text": "     `);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1deea",
      "text": "   ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1deeb",
      "text": "     let vscript = document.createElement(\"script\");",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1deec",
      "text": "     vscript.id = this.id;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1deed",
      "text": "     vscript.src = `https://scrapbox.io/api/code//${scrapbox.Project.name}/${encodeURIComponent(scrapbox.Page.title)}/visScript.js` ;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1deee",
      "text": "     document.body.appendChild(vscript);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1deef",
      "text": "   } ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def0",
      "text": "   hasScript() { return document.getElementById(this.id) !== null;}",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def1",
      "text": "        ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def2",
      "text": "   remove() {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def3",
      "text": "     const vscript = document.getElementById(this.id);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def4",
      "text": "     if (!vscript) return;",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def5",
      "text": "     // 既にscriptがある場合は削除する",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def6",
      "text": "     vscript.parentNode.removeChild(vscript);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def7",
      "text": "     console.log('The script already exists. Removing it...');",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def8",
      "text": "   }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1def9",
      "text": " }",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1defa",
      "text": "",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1defb",
      "text": "code:visScript.js",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1defc",
      "text": " // create an array with nodes",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1defd",
      "text": " var nodes = new vis.DataSet([",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1defe",
      "text": "   { id: 1, label: \"ノード 1\" },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1deff",
      "text": "   { id: 2, label: \"Node 2\" },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df00",
      "text": "   { id: 3, label: \"Node 3\" },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df01",
      "text": "   { id: 4, label: \"Node 4\" },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df02",
      "text": "   { id: 5, label: \"Node 5\" },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df03",
      "text": " ]);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df04",
      "text": " ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df05",
      "text": " // create an array with edges",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df06",
      "text": " var edges = new vis.DataSet([",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df07",
      "text": "   { from: 1, to: 3 },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df08",
      "text": "   { from: 1, to: 2 },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df09",
      "text": "   { from: 2, to: 4 },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df0a",
      "text": "   { from: 2, to: 5 },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df0b",
      "text": "   { from: 3, to: 3 },",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df0c",
      "text": " ]);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df0d",
      "text": " ",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df0e",
      "text": " // create a network",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df0f",
      "text": " var container = document.getElementById(\"vis\");",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df10",
      "text": " var data = {",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df11",
      "text": "   nodes: nodes,",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df12",
      "text": "   edges: edges,",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df13",
      "text": " };",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df14",
      "text": " var options = {};",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
    {
      "id": "5fd0d5e55b98c20000a1df15",
      "text": " var network = new vis.Network(container, data, options);",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1607521766,
      "updated": 1607521766
    },
