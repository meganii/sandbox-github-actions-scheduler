    {
      "id": "659218c62520d5002469db77",
      "text": "マンダラート生成コード",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a0",
      "text": "from [井戸端マンダラート2024]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a1",
      "text": "マンダラート生成コード",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a2",
      "text": "書き初め2024[nishio.icon]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a3",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a4",
      "text": " base = [",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a5",
      "text": "     [\"1\", \"2\", \"3\"],",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a6",
      "text": "     [\"4\", \"\", \"5\"],",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a7",
      "text": "     [\"6\", \"7\", \"8\"],",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a8",
      "text": " ]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5a9",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5aa",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5ab",
      "text": " def f(x):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5ac",
      "text": "     def concat(b):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5ad",
      "text": "         if not x:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5ae",
      "text": "             return b",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5af",
      "text": "         if not b:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5b0",
      "text": "             return f\"[[{x}]]\"",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5b1",
      "text": "         return f\"{x}-{b}\" if x else b",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5b2",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5b3",
      "text": "     return apply_tree(base, concat) ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704079923
    },
    {
      "id": "65923234aff09e0000f804c7",
      "text": "\t",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704079925,
      "updated": 1704079926
    },
    {
      "id": "659218c6aff09e000094a5ba",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5bb",
      "text": " def apply_tree(tree, func):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5bc",
      "text": "     result = []",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5bd",
      "text": "     for e in tree:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5be",
      "text": "         if isinstance(e, str):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5bf",
      "text": "             result.append(func(e))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c0",
      "text": "         else:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c1",
      "text": "             result.append(apply_tree(e, func))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c2",
      "text": "     return result",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c3",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c4",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c5",
      "text": " def print_tree_matrix(tree):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c6",
      "text": "     for layer in tree:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c7",
      "text": "         for row in zip(*layer):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c8",
      "text": "             print(\"\\t\".join(sum(row, [])))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5c9",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5ca",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5cb",
      "text": " print_tree_matrix(apply_tree(base, f))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
      "id": "659218c6aff09e000094a5cc",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1704073415,
      "updated": 1704073415
    },
    {
