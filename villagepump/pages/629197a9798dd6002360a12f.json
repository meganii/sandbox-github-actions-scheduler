    {
      "id": "629197b05b98c2000034fe52",
      "text": "real programmer",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1653708720,
      "updated": 1653708737
    },
    {
      "id": "629197c25b98c2000034fe53",
      "text": "真のプログラマと訳す場合もある",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1653708738,
      "updated": 1653708770
    },
    {
      "id": "629197ab5b98c2000034fe50",
      "text": "https://wiki.c2.com/?RealProgrammer",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1653708715,
      "updated": 1653708716
    },
    {
      "id": "629197ac5b98c2000034fe51",
