    {
      "id": "6245cae14bb2e2000071d0de",
      "text": "ビルドを試みている[kuuote.icon]",
      "userId": "5e8944948a182900174bb2e2",
      "created": 1648741034,
      "updated": 1648741041
    },
    {
      "id": "6245cae84bb2e2000071d0e0",
      "text": " 環境はArch Linux",
      "userId": "5e8944948a182900174bb2e2",
      "created": 1648741041,
      "updated": 1648741044
    },
    {
      "id": "6245caeb4bb2e2000071d0e1",
      "text": " Perlのライブラリが足りないので入れた",
      "userId": "5e8944948a182900174bb2e2",
      "created": 1648741045,
      "updated": 1648741052
    },
    {
      "id": "6245cafc4bb2e2000071d0e4",
