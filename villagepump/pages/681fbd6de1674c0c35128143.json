    },
    {
      "id": "6820a8810000000000bab226",
      "text": "[nishio.icon]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1746970753,
      "updated": 1746970760
    },
    {
      "id": "6820a8880000000000bab227",
      "text": " 一仕事終わった",
      "userId": "582e63d27c56960011aff09e",
      "created": 1746970760,
      "updated": 1746970765
    },
    {
      "id": "6820a8950000000000bab228",
