      "created": 1730882373,
      "updated": 1730882373
    },
    {
      "id": "6651b41871b3c20000224c3a",
      "text": "[** 必ず指定]",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1716630553,
      "updated": 1716630560
    },
    {
      "id": "6651b3c571b3c2000081fe61",
      "text": "code:各ユーザーで指定する.css",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1716630470,
      "updated": 1716630481
    },
    {
      "id": "6651b3ca71b3c2000081fe63",
      "text": "\t.line{",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1716630474,
      "updated": 1716630475
    },
    {
      "id": "6651b3cb71b3c2000081fe64",
      "text": "\t--indent-width-usercss: ; /* 各インデントの幅 */",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1716630475,
      "updated": 1716630490
    },
    {
      "id": "6651b3cb71b3c2000081fe65",
      "text": "\t}",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1716630475,
      "updated": 1716630475
    },
    {
      "id": "6651b3c671b3c2000081fe62",
      "text": "[** CSS本体]",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1716630470,
      "updated": 1725259907
    },
    {
      "id": "6628b97b71b3c20000488331",
      "text": "code:style.css",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1713944956,
      "updated": 1713944956
    },
    {
      "id": "6628bc2471b3c2000079c76a",
      "text": "\t/* まずはドットから */ ",
      "userId": "661b6e882a96e9002371b3c2",
      "created": 1713945637,
      "updated": 1713945649
    },
    {
