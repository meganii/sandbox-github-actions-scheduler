    },
    {
      "id": "5fc3c07c8ee92a0000b69383",
      "text": "[https://gyazo.com/67b759de8d9eb7a092a0161b57e27512]",
      "userId": "566fa156776c9b11008ee92a",
      "created": 1606664317,
      "updated": 1606664317
    },
    {
      "id": "5fc3c07c8ee92a0000b69384",
      "text": "",
