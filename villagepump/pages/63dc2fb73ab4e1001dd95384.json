    },
    {
      "id": "63dc305071fa08000071514e",
      "text": "歌詞",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374673,
      "updated": 1675374675
    },
    {
      "id": "63dc2fe071fa080000715135",
      "text": "> ①ぼくらはみんな　生きている",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374560,
      "updated": 1675374672
    },
    {
      "id": "63dc303a71fa080000715137",
      "text": "> 　生きているから　歌うんだ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715138",
      "text": "> 　ぼくらはみんな　生きている",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715139",
      "text": "> 　生きているから　かなしいんだ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa08000071513a",
      "text": "> ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa08000071513b",
      "text": "> 　手のひらを太陽に　すかしてみれば",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa08000071513c",
      "text": "> 　真っ赤に流れる　ぼくの血潮",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa08000071513d",
      "text": "> ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa08000071513e",
      "text": "> 　ミミズだって　オケラだって　アメンボだって",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa08000071513f",
      "text": "> 　みんな　みんな生きているんだ　友だちなんだ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715140",
      "text": "> ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715141",
      "text": "> ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715142",
      "text": "> ②ぼくらはみんな　生きている",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715143",
      "text": "> 　生きているから　笑うんだ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715144",
      "text": "> 　ぼくらはみんな　生きている",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715145",
      "text": "> 　生きているから　うれしいんだ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715146",
      "text": "> ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715147",
      "text": "> 　手のひらを太陽に　すかしてみれば",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715148",
      "text": "> 　まっかに流れる　ぼくの血潮",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa080000715149",
      "text": "> ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa08000071514a",
      "text": "> 　トンボだって　カエルだって　ミツバチだって",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa08000071514b",
      "text": "> 　みんな　みんな生きているんだ　友だちなんだ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc303a71fa08000071514c",
      "text": "> ",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374650,
      "updated": 1675374650
    },
    {
      "id": "63dc2fe071fa080000715136",
      "text": "",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374561,
      "updated": 1675374561
    },
    {
      "id": "63dc306a71fa08000071514f",
      "text": "[生命]を感じる[biwa.icon]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675374699,
      "updated": 1675374705
    },
    {
      "id": "63dc33b0b8bbfc00006caef1",
      "text": "[オケラ]って何だよとずっと思っていた[はるひ.icon]",
      "userId": "62c8e96f1f5ebf001d71fa08",
      "created": 1675375536,
      "updated": 1675376354
    },
    {
      "id": "63dc33e8b8bbfc00006caef2",
      "text": "\tいや、今もよく知らない",
      "userId": "60c9f2f6c225a50020b8bbfc",
      "created": 1675375593,
      "updated": 1675375604
    },
    {
      "id": "63dc36f771fa080000716b62",
      "text": "\tなんか、田んぼにいる、虫[biwa.icon]",
      "userId": "62c8e96f1f5ebf001d71fa08",
