    },
    {
      "id": "62d68de00553320000f3707c",
      "text": "モニターアーム",
      "userId": "5d131fa66fe2890017055332",
      "created": 1658228193,
      "updated": 1658228196
    },
    {
      "id": "62d68de30553320000f3707d",
      "text": "ディスプレイアーム",
      "userId": "5d131fa66fe2890017055332",
      "created": 1658228196,
      "updated": 1658228202
    },
    {
      "id": "62d68dec0553320000f3707f",
      "text": "　どっちもある、、通販サイトだと「ディスプレイ（モニター）」とか併記されていたりする",
      "userId": "5d131fa66fe2890017055332",
      "created": 1658228205,
      "updated": 1658228238
    },
    {
