    },
    {
      "id": "678b61a271b3c20000a1e70f",
      "text": "$ tar -c code -N \"$(stat \"$HOME/codebu/$(ls ~/codebu --time=birth | tail -n 1)\" -c %w)\" | zstd --ultra -22 -T0 -o \"$HOME/codebu/$(date -Iminutes -u).tar.zst\"",
