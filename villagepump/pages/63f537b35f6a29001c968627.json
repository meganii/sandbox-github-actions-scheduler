      "userId": "58442db3ee4a2400116eb406",
      "created": 1677111929,
      "updated": 1677111931
    },
    {
      "id": "63f6b27f6eb40600006a19ce",
      "text": "　　\t2/27開催にするか",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1677111936,
      "updated": 1677111943
    },
    {
      "id": "63f6b2a36eb40600006a19cf",
      "text": "　　\t[20230227ボイチャ議事録]",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1677111971,
      "updated": 1677111980
    },
    {
      "id": "63f5dbfa6eb4060000a87518",
