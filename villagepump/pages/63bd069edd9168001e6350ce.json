      "updated": 1673594189
    },
    {
      "id": "63bfb7a2774b170000472ced",
      "text": "[基素.icon]",
      "userId": "59fb21121207900012774b17",
      "created": 1673508771,
      "updated": 1673508772
    },
    {
      "id": "63bfb7a5774b170000472cee",
      "text": " 上の解説をまだ理解できていない",
      "userId": "59fb21121207900012774b17",
      "created": 1673508773,
      "updated": 1673508782
    },
