    {
      "id": "653232d95e90c00000a65e60",
      "text": "学級",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1697788633,
      "updated": 1697788636
    },
    {
      "id": "653232dd5e90c00000a65e62",
      "text": "　黒鷲の学級",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1697788636,
      "updated": 1697788645
    },
    {
      "id": "653232e65e90c00000a65e63",
      "text": "　青獅子の学級",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1697788646,
      "updated": 1697788670
    },
    {
      "id": "653232fe5e90c00000a65e64",
      "text": "　金鹿の学級",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1697788670,
      "updated": 1697788682
    },
    {
      "id": "653232d95e90c00000a65e61",
      "text": "",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1697788633,
      "updated": 1697788633
