      "created": 1603792091,
      "updated": 1611913471
    },
    {
      "id": "6013d8fed438750000f39083",
      "text": " like\tname\tprice",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913471,
      "updated": 1611913471
    },
    {
      "id": "6013d8fed438750000f39084",
      "text": " 699\t[まるで完熟マンゴー]　1本入\t138",
      "userId": "6055950b87138f0022246f59",
      "created": 1611913471,
      "updated": 1620033723
    },
    {
      "id": "6013d8fed438750000f39085",
      "text": " 532\tいちご練乳氷\t149",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913471,
      "updated": 1611913471
    },
    {
      "id": "6013d8fed438750000f39086",
      "text": " 451\t抹茶ラテバー　1本入\t149",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913471,
      "updated": 1611913471
    },
    {
      "id": "6013d8fed438750000f39087",
      "text": " 442\t抹茶練乳氷　160ml\t149",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913471,
      "updated": 1611913471
    },
    {
      "id": "5f9806f4a9f4740000a8d06d",
      "text": "このあと　家族全員分　マンゴーアイス買った　最高　今年一番の味",
      "userId": "5f7bafa76f34a0001ea9f474",
      "created": 1603798772,
      "updated": 1603961714
    },
    {
      "id": "5fbfe6274daa92000025e6a8",
      "text": "code:.py",
      "userId": "5bc5772df30ff700134daa92",
      "created": 1606411816,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39108",
      "text": " \"\"\"セブンプレミアムの商品をすべて取得\"\"\"",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39109",
      "text": " from selenium import webdriver",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3910a",
      "text": " import chromedriver_binary",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3910b",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3910c",
      "text": " def pick():",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3910d",
      "text": "     options = webdriver.ChromeOptions()",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3910e",
      "text": "     driver = webdriver.Chrome(options=options)",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3910f",
      "text": "     driver_path = '/app/.chromedriver/bin/chromedriver'",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39110",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39111",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39112",
      "text": "     driver.get(\"https://7premium.jp/product/search?item_tag_category_id=19\")",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39113",
      "text": "     while True:",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39114",
      "text": "         with open(\"merchandises\",'a') as f:",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39115",
      "text": "             for _ in driver.find_elements_by_class_name(\"itemList_item\"):",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39116",
      "text": "                 f.write(_.text)",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39117",
      "text": "         driver.find_element_by_css_selector(",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39118",
      "text": "             \"#js-itemList > nav > ul > li.pager_item.pager_item-next\").click()",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39119",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3911a",
      "text": " pick()",
      "userId": "6055950b87138f0022246f59",
      "created": 1611913495,
      "updated": 1620033959
    },
    {
      "id": "6013d915d438750000f3911b",
      "text": " f = open(\"merchandises\", \"r\")",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3911c",
      "text": " u = f.readlines()",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3911d",
      "text": " n = \"\".join(u).split(\"円\")",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3911e",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f3911f",
      "text": " for _ in n:",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
    {
      "id": "6013d915d438750000f39120",
      "text": "     print(_.replace(\"\\n\",\",\").replace(\"税込\",\",\").replace(\",,\",\",\"))",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611913495,
      "updated": 1611913495
    },
