    {
      "id": "6804f842377947aa5413deb0",
      "text": "アサーション",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156195,
      "updated": 1745156195
    },
    {
      "id": "680510d10000000000338ecb",
      "text": "assertion",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1745162449,
      "updated": 1745162452
    },
    {
      "id": "6804f86200000000000afa47",
      "text": "心理学におけるアサーション",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156195,
      "updated": 1745156200
    },
    {
      "id": "6804f86600000000000afa49",
      "text": " 自分の気持ちや考えを相手に伝えつつ、",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156200,
      "updated": 1745156202
    },
    {
      "id": "6804f86900000000000afa4a",
      "text": " 相手の気持ちも尊重するコミュニケーション方法",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156202,
      "updated": 1745156211
    },
    {
      "id": "6804f86c00000000000afa4b",
      "text": " より良い人間関係を築くために役立つスキル﻿",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156205,
      "updated": 1745156208
    },
    {
      "id": "6804f87500000000000afa4c",
      "text": "",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156215,
      "updated": 1745156218
    },
    {
      "id": "6804f87900000000000afa4d",
      "text": "コンピュータのアサーションって",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156218,
      "updated": 1745156295
    },
    {
      "id": "6804f8c500000000000afa4e",
      "text": " 事前に条件を明示化することで",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156295,
      "updated": 1745156296
    },
    {
      "id": "6804f8c700000000000afa4f",
      "text": " その後のエラーなく気持ち良い動作をさせるため",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156296,
      "updated": 1745156296
    },
    {
      "id": "6804f8c800000000000afa50",
      "text": " ってことか",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156297,
      "updated": 1745156300
    },
    {
      "id": "6804f86200000000000afa48",
      "text": "",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156195,
      "updated": 1745156195
    },
    {
      "id": "6804fa0f00000000000afa51",
      "text": "[架空のアサーションエピソード]",
      "userId": "6436266c7ac3c0001bed60e6",
      "created": 1745156625,
      "updated": 1745156638
    },
    {
      "id": "680514d40000000000e01e3c",
      "text": "[アサーショントレーニング]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1745163476,
      "updated": 1745163484
    },
    {
      "id": "6804fa1500000000000afa52",
      "text": "[アサーティブネス]",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1745156631,
      "updated": 1745162497
    }
  ]
}