    {
      "id": "645a676db16e6b72f4d0a04a",
      "text": "自分について語るとき、自分の言葉を用いて自分を定義する",
      "userId": "638c3fccacf7a9001d313fe7",
      "created": 1683646317,
      "updated": 1683646317
    },
    {
      "id": "645a676d313fe7000004955c",
      "text": "from [自分語り]",
      "userId": "638c3fccacf7a9001d313fe7",
      "created": 1683646317,
      "updated": 1683646317
    },
    {
      "id": "645a6782313fe7000098133e",
      "text": "by [kyamad9.icon]",
      "userId": "638c3fccacf7a9001d313fe7",
      "created": 1683646338,
      "updated": 1683646341
    },
    {
      "id": "645a6782313fe7000098133f",
