  "lines": [
    {
      "id": "60144d9b82d49e001c3d0c7b",
      "text": "連体修飾語",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943354,
      "updated": 1611943354
    },
    {
      "id": "60144dbae5172d0000d5009a",
      "text": "\t[修飾語]の中で[体言]を修飾するもの",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943354,
      "updated": 1611943549
    },
    {
      "id": "60144e7ce5172d0000d500a2",
      "text": "\t[名詞句]ができる",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943549,
      "updated": 1611943574
    },
    {
      "id": "60144eece5172d0000d500a3",
      "text": "",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943661,
      "updated": 1611943661
    },
    {
      "id": "60144eece5172d0000d500a4",
      "text": "連体修飾語になる品詞",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943661,
      "updated": 1611943661
    },
    {
      "id": "60144eece5172d0000d500a5",
      "text": " 格助詞「の」",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943661,
      "updated": 1611943670
    },
    {
      "id": "60144ef6e5172d0000d500ab",
      "text": " \t「私のペン」",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943670,
      "updated": 1611943670
    },
    {
      "id": "60144eece5172d0000d500a6",
      "text": " 形容詞の[連体形]",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943661,
      "updated": 1611943808
    },
    {
      "id": "60144ef8e5172d0000d500ac",
      "text": " \t「美しい絵」",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943672,
      "updated": 1611943673
    },
    {
      "id": "60144eece5172d0000d500a7",
      "text": " 形容動詞の連体形",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943661,
      "updated": 1611943700
    },
    {
      "id": "60144efae5172d0000d500ad",
      "text": " \t「きれいな花」",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943675,
      "updated": 1611943675
    },
    {
      "id": "60144eece5172d0000d500a8",
      "text": " 連体詞",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943661,
      "updated": 1611943677
    },
    {
      "id": "60144efce5172d0000d500ae",
      "text": " \t「この本」",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943677,
      "updated": 1611943677
    },
    {
      "id": "60144eece5172d0000d500a9",
      "text": " 用言または活用連語の連体形",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943661,
      "updated": 1611943680
    },
    {
      "id": "60144f00e5172d0000d500af",
      "text": " \t「飛ぶ鳥」「咲かない花」「気取った言葉遣い」",
      "userId": "5c30ba88e98efe0017e5172d",
      "created": 1611943680,
      "updated": 1611943681
    },
