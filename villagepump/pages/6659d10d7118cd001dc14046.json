      "updated": 1717163848
    },
    {
      "id": "6863f4f800000000004260a0",
      "text": "一年は365日",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381241,
      "updated": 1751381244
    },
    {
      "id": "6863f4fc00000000004260a2",
      "text": "　真ん中は182.5日",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381244,
      "updated": 1751381249
    },
    {
      "id": "6863f50200000000004260a4",
      "text": "　1月：31日",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381250,
      "updated": 1751381275
    },
    {
      "id": "6863f50b00000000004260a5",
      "text": "　2月：27日",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381259,
      "updated": 1751381267
    },
    {
      "id": "6863f51300000000004260a6",
      "text": "　3月：31日",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381267,
      "updated": 1751381277
    },
    {
      "id": "6863f51f00000000004260a7",
      "text": "　4月：30日",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381279,
      "updated": 1751381283
    },
    {
      "id": "6863f52700000000004260a8",
      "text": "　5月：31日",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381288,
      "updated": 1751381292
    },
    {
      "id": "6863f52c00000000004260a9",
      "text": "　6月：30日",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381292,
      "updated": 1751381295
    },
    {
      "id": "6863f52f00000000004260aa",
      "text": "　合計：[% 31+27+31+30+31+30] = 180日",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381296,
      "updated": 1751381324
    },
    {
      "id": "6863f54b00000000004260ab",
      "text": "　なので7/2正午が一年の真ん中",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381324,
      "updated": 1751381344
    },
    {
      "id": "6863f4fa00000000004260a1",
      "text": "",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381242,
      "updated": 1751381242
    },
    {
      "id": "6863f56300000000004260ac",
      "text": "うるう年だと366日、183日目なので7/2と7/3が切り替わる瞬間が真ん中",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381347,
      "updated": 1751381407
    },
    {
      "id": "6863f56400000000004260ad",
      "text": "",
      "userId": "64099110a39a5c001c8a016e",
      "created": 1751381349,
      "updated": 1751381349
    }
  ]
}