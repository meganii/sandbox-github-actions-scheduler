  "lines": [
    {
      "id": "62aaabf0f753ff001da0aac6",
      "text": "miro next japan",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352352,
      "updated": 1655352352
    },
    {
      "id": "62aaace1caa1660000da21be",
      "text": "　日時:2022年6月16日(木)13:00~15:00",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352544,
      "updated": 1655352548
    },
    {
      "id": "62aaace1caa1660000da21bf",
      "text": "　視聴URL: Live配信 ",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352544,
      "updated": 1655359113
    },
    {
      "id": "62aaac23caa1660000da21b6",
      "text": "　参加費用: 無料 ",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352354,
      "updated": 1655352552
    },
    {
      "id": "62aaace9caa1660000da21c0",
      "text": "　　メモ代わりに掲載",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352552,
      "updated": 1655352554
    },
    {
      "id": "62aaac42caa1660000da21b8",
      "text": " 　日本ローカルテンプレート公開",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352385,
      "updated": 1655352403
    },
    {
      "id": "62aaac54caa1660000da21b9",
      "text": " 　ユーザーのベストプラクティステンプレートを公開＝ミロユニバース",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352403,
      "updated": 1655352440
    },
    {
      "id": "62aaac80caa1660000da21ba",
      "text": " 　　ほほぉ。あとでみてみよう[shotato.icon]",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352447,
      "updated": 1655359268
    },
    {
      "id": "62aaac8acaa1660000da21bb",
      "text": " 　　考え方の地域性が如実にでる可能性があっておもしろいかもしれん",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352457,
      "updated": 1655352483
    },
    {
      "id": "62aaaca4caa1660000da21bc",
      "text": " 　日本語UI公開　6/13より",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352483,
      "updated": 1655352511
    },
    {
      "id": "62aaacc2caa1660000da21bd",
      "text": " 　NEC　アジャイル開発フローMiro上に公開",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352513,
      "updated": 1655352634
    },
    {
      "id": "62aaadd1caa1660000da21c3",
      "text": "",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655352783,
      "updated": 1655352783
    },
    {
      "id": "62aab6dfcaa1660000da21c6",
      "text": "　ポールダーシー",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355102,
      "updated": 1655355124
    },
    {
      "id": "62aab6f7caa1660000da21c7",
      "text": "　　パンデミックにより仕事時間が長くなった",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355126,
      "updated": 1655355334
    },
    {
      "id": "62aab7c7caa1660000a0c633",
      "text": "　　ベッドから仕事場まで１６歩",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355336,
      "updated": 1655355349
    },
    {
      "id": "62aab7d4caa1660000a0c634",
      "text": "　　会議の時間の割合が増える",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355349,
      "updated": 1655355368
    },
    {
      "id": "62aab7f2caa1660000a0c637",
      "text": "　　　生産性を下げている",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355379,
      "updated": 1655355404
    },
    {
      "id": "62aab80ccaa1660000a0c638",
      "text": "　　現代人はデジタルツールの中に住んでいる",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355405,
      "updated": 1655355447
    },
    {
      "id": "62aab91ccaa1660000a0c63f",
      "text": "　　時代の変化により下記が求められる",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355677,
      "updated": 1655355702
    },
    {
      "id": "62aab83bcaa1660000a0c63b",
      "text": " 　　同期非同期両方の対応",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355451,
      "updated": 1655359223
    },
    {
      "id": "62aab902caa1660000a0c63c",
      "text": " 　　あらゆるツールと統合",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355650,
      "updated": 1655355675
    },
    {
      "id": "62aab90bcaa1660000a0c63d",
      "text": " 　　各構成カスタマイズ性",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355660,
      "updated": 1655355675
    },
    {
      "id": "62aab911caa1660000a0c63e",
      "text": " 　　直感的",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355666,
      "updated": 1655355711
    },
    {
      "id": "62aab93ecaa1660000a0c640",
      "text": " 　Zoomからミロを起動、一緒に開くを押すと同期できる。Webex",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655355711,
      "updated": 1655356049
    },
    {
      "id": "62aaba93caa1660000a0c641",
      "text": " 　Google　ドキュメント、シートなどミロに挿入、相互編集可",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356052,
      "updated": 1655356090
    },
    {
      "id": "62aabab9caa1660000a0c642",
      "text": " 　　googleファイルのつながりを視覚化できる",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356090,
      "updated": 1655356124
    },
    {
      "id": "62aabadfcaa1660000a0c643",
      "text": " 　カレンダーにもMiroを追加可",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356128,
      "updated": 1655356151
    },
    {
      "id": "62aabbefcaa1660000a0c644",
      "text": " 　プレゼンを録画",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356400,
      "updated": 1655356408
    },
    {
      "id": "62aabbf8caa1660000a0c645",
      "text": " 　　非同期で確認可能、倍速再生可、キャッチアップしやすい",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356409,
      "updated": 1655356447
    },
    {
      "id": "62aabc16caa1660000a0c646",
      "text": " 　フレームワーク",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356438,
      "updated": 1655356917
    },
    {
      "id": "62aabdf4caa1660000a0c647",
      "text": " 　　REST API",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356917,
      "updated": 1655356931
    },
    {
      "id": "62aabe04caa1660000a0c648",
      "text": " 　　Web SDK",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356933,
      "updated": 1655356946
    },
    {
      "id": "62aabe0dcaa1660000a0c649",
      "text": " 　　ライブ埋め込み",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356942,
      "updated": 1655356955
    },
    {
      "id": "62aabe34caa1660000a0c64a",
      "text": " 　　　Notionへの埋め込み可",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655356981,
      "updated": 1655357000
    },
    {
      "id": "62aabe48caa1660000a0c64b",
      "text": " 　　Miroアプリ作成、共有可",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655357001,
      "updated": 1655357130
    },
    {
      "id": "62aabecacaa1660000a0c64c",
      "text": " 　Miroプラットフォーム",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655357130,
      "updated": 1655357152
    },
    {
      "id": "62aabedfcaa1660000a0c64d",
      "text": " 　　ex)スプレッドシートから組織図を簡単に作成",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655357152,
      "updated": 1655357228
    },
    {
      "id": "62aabf1bcaa1660000a0c64e",
      "text": " 　　ボードの公開非公開をリスト化するアプリ等サンプルライブラリに公開中",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655357212,
      "updated": 1655357488
    },
    {
      "id": "62aac72acaa1660000abde07",
      "text": " 　プログラミングに疎いがAPIいじれるようになると便利そうだな",
      "userId": "628449f5dda6e60023caa166",
      "created": 1655359275,
      "updated": 1655359314
    },
