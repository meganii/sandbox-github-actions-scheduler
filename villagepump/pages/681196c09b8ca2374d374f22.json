    },
    {
      "id": "6811a8cd0000000000d92c61",
      "text": "[ネチケット]",
      "userId": "67c9668a75b2b196fa99a913",
      "created": 1745987790,
      "updated": 1745987792
    },
    {
      "id": "6811a8ce0000000000d92c62",
      "text": "",
