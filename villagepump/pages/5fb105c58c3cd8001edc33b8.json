      "created": 1605436901,
      "updated": 1605436901
    },
    {
      "id": "5fb105e5d4387500004cc778",
      "text": "スマホ出たばかりの頃",
      "userId": "5fa258bdb102120024d43875",
      "created": 1605436902,
      "updated": 1605436912
    },
    {
      "id": "5fb105f5d4387500004cc77b",
      "text": " なんか新しいアプリというだけで楽しかった",
      "userId": "5fa258bdb102120024d43875",
      "created": 1605436918,
      "updated": 1605436936
    },
    {
      "id": "5fb10664d4387500004cc77e",
      "text": "",
      "userId": "5fa258bdb102120024d43875",
      "created": 1605437029,
      "updated": 1605437029
    },
    {
      "id": "5fb10664d4387500004cc77f",
      "text": "iOS6までと7以降",
      "userId": "5fa258bdb102120024d43875",
      "created": 1605437029,
      "updated": 1605437044
    },
    {
      "id": "5fb10673d4387500004cc780",
      "text": "",
      "userId": "5fa258bdb102120024d43875",
