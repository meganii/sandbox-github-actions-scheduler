    {
      "id": "65328a145e90c000005184fd",
      "text": "[大江千里](歌人の方)",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1697810964,
      "updated": 1697810978
    },
    {
      "id": "65328aa85e90c00000518502",
      "text": "　古今和歌集の秋歌上、192",
      "userId": "60508ed36a7abc00235e90c0",
      "created": 1697811112,
      "updated": 1697811194
    },
    {
      "id": "65328a155e90c000005184fe",
      "text": "",
