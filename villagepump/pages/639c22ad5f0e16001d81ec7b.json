    {
      "id": "639c22b3a9108b0000450dd5",
      "text": "[chatGPT]でできること",
      "userId": "5fb29108ce01660038a9108b",
      "created": 1671176884,
      "updated": 1671177430
    },
    {
      "id": "639c22baa9108b0000450ddb",
      "text": "　アイデアを出させる",
      "userId": "5fb29108ce01660038a9108b",
      "created": 1671176890,
      "updated": 1671176934
    },
    {
      "id": "639c22d1a9108b0000450ddf",
      "text": "　質問内容を出させる",
      "userId": "5fb29108ce01660038a9108b",
      "created": 1671176913,
      "updated": 1671176938
    },
    {
      "id": "639c22c7a9108b0000450dde",
      "text": " 　[ChatGPTは質問をしてくれる]",
      "userId": "5fb29108ce01660038a9108b",
      "created": 1671176904,
      "updated": 1671176917
    },
    {
      "id": "639c22baa9108b0000450ddc",
      "text": "　困りごとのアドバイスを答えさせる",
      "userId": "5fb29108ce01660038a9108b",
      "created": 1671176890,
      "updated": 1671176932
    },
    {
      "id": "639c22d5a9108b0000450de0",
      "text": "　文章を要約する",
      "userId": "5fb29108ce01660038a9108b",
      "created": 1671176918,
      "updated": 1671177248
    },
    {
      "id": "639c2422a9108b0000450ded",
      "text": "　　[ChatGPTで文章を要約する]",
