    {
      "id": "608e7104b3205f001d21691d",
      "text": "エビングハウス錯視",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947781,
      "updated": 1619947781
    },
    {
      "id": "608e710555cbd300000a0e07",
      "text": "[錯視]",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947781,
      "updated": 1619947784
    },
    {
      "id": "608e720d55cbd300000a0e16",
      "text": " 名前は[忘却曲線]の[ヘルマン・エビングハウス]から",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619948046,
      "updated": 1619948057
    },
    {
      "id": "608e713d55cbd300000a0e0a",
      "text": "",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947838,
      "updated": 1619947838
    },
    {
      "id": "608e713e55cbd300000a0e0b",
      "text": "[https://gyazo.com/f7b681eb90faa67bb235d660a62a7b59]",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947839,
      "updated": 1619947839
    },
    {
      "id": "608e714855cbd300000a0e0e",
      "text": "",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947848,
      "updated": 1619947848
    },
    {
      "id": "608e713f55cbd300000a0e0d",
      "text": "オレンジの円は同じ大きさ",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947839,
      "updated": 1619947848
    },
    {
      "id": "608e716355cbd300000a0e0f",
      "text": " [ヒト]は周囲の円が小さい方がオレンジの円が大きく見える",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947876,
      "updated": 1619948035
    },
    {
      "id": "608e71f855cbd300000a0e15",
      "text": " [イルカ]はヒトと同じ錯視が生じているらしい",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619948025,
      "updated": 1619948034
    },
    {
      "id": "608e717455cbd300000a0e10",
      "text": " [ハト]はヒトとは逆に周囲が大きいほうが大きく見えているらしい",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947892,
      "updated": 1619948356
    },
    {
      "id": "608e719c55cbd300000a0e11",
      "text": "  ハトは[ミュラー・リヤー錯視]も生じていない",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947932,
      "updated": 1619947971
    },
    {
      "id": "608e71c255cbd300000a0e13",
      "text": " [ギニアヒヒ]では生じない",
      "userId": "5ae7fecf7766b7001455cbd3",
      "created": 1619947971,
      "updated": 1619948038
    },
    {
