  "lines": [
    {
      "id": "6374b5f99e9c080023e8e4b0",
      "text": "天気図を表示するUserCSS",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668593148,
      "updated": 1668593175
    },
    {
      "id": "6374b5fc5b98c20000511817",
      "text": "code:style.css",
      "userId": "5b5e7121417cf700145b98c2",
      "created": 1668593148,
      "updated": 1668593167
    },
    {
      "id": "6374b60f5b98c20000511818",
      "text": " @media only screen and (min-width: 480px) {",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1668593167,
      "updated": 1714630241
    },
    {
      "id": "66332e611280f0000069312d",
      "text": "   a[type=\"hashTag\"][href=\"/villagepump/%E5%A4%A9%E6%B0%97%E5%9B%B3\"]::after {",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630252
    },
    {
      "id": "66332e611280f0000069312e",
      "text": "     --scale: 0.5;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f0000069312f",
      "text": "     --width: calc(600px * var(--scale));",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693130",
      "text": "     --height: calc(581px * var(--scale));",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693131",
      "text": "     ",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693132",
      "text": "     position: fixed;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693133",
      "text": "     top: 40px;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693134",
      "text": "     right: 0;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693135",
      "text": "     display: block;",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693136",
      "text": "     width: var(--width);",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693137",
      "text": "     height: var(--height);",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693138",
      "text": "     content: \"\";",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f00000693139",
      "text": "     background-image: url(\"https://takker-weather.vercel.app/jma/weather-map/archives/today.png\");",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f0000069313a",
      "text": "     background-size: var(--width) var(--height);",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630274
    },
    {
      "id": "66332e611280f0000069313b",
      "text": "   }",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630265
    },
    {
      "id": "66332e611280f0000069313c",
      "text": " }",
      "userId": "5ef2bdebb60650001e1280f0",
      "created": 1714630241,
      "updated": 1714630241
    },
