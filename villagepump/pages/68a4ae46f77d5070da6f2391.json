      "created": 1755625536,
      "updated": 1755625544
    },
    {
      "id": "68a4b84a000000000065710e",
      "text": "   フォアグラ殺し",
      "userId": "640b5ab62ffe46001b897bef",
      "created": 1755625546,
      "updated": 1755625579
    },
    {
      "id": "68a4ba00000000000065711d",
      "text": "    そもそもフォアグラが怖い",
      "userId": "640b5ab62ffe46001b897bef",
      "created": 1755625984,
      "updated": 1755625991
    },
    {
      "id": "68a4b8fc0000000000657119",
      "text": "   人間の皮膚コレクター",
      "userId": "640b5ab62ffe46001b897bef",
      "created": 1755625724,
      "updated": 1755625735
    },
    {
      "id": "68a4b86b0000000000657111",
      "text": "  仮面ライダー鎧武",
      "userId": "640b5ab62ffe46001b897bef",
      "created": 1755625579,
      "updated": 1755625592
